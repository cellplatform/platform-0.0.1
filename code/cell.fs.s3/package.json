{
  "name": "@platform/cell.fs.s3",
  "version": "0.4.146",
  "description": "S3 (AWS) filesystem support for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "node .",
    "watch": "nodemon .",
    "test": "ts test",
    "tdd": "ts test --watch",
    "test:integration": "ts test --suffix INTEGRATION",
    "tddi": "ts test --suffix INTEGRATION --watch",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "prepare": "ts prepare"
  },
  "dependencies": {
    "@platform/cell.fs": "0.4.141",
    "@platform/cell.schema": "0.5.19",
    "@platform/cell.types": "0.5.20",
    "@platform/fs.s3": "0.7.35",
    "@platform/libs": "0.2.3"
  },
  "devDependencies": {
    "@platform/fs": "0.6.0",
    "@platform/test": "0.1.17",
    "@platform/ts": "4.0.6"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
