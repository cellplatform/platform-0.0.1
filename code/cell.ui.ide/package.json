    "prepare": "ts prepare"
  },
  "dependencies": {
    "@monaco-editor/react": "3.7.3",
    "@platform/cell.client": "0.3.39",
    "@platform/cell.schema.sys": "0.0.88",
    "@platform/cell.types": "0.5.49",
    "@platform/cell.typesystem": "0.0.168",
    "@platform/cell.ui": "0.1.179",
    "@platform/libs": "0.2.4",
    "@platform/react": "0.4.60",
    "@platform/state": "0.7.25",
    "@platform/types": "0.5.13",
    "@platform/ui.icon": "0.0.179",
    "monaco-themes": "0.3.3",
    "react-icons": "4.1.0"
  },
  "devDependencies": {
    "@platform/test": "0.2.1",
    "@platform/ts": "4.1.3",
    "@platform/ui.dev": "0.0.34"
