{
  "name": "@platform/cell.ui.ide",
  "version": "0.0.130",
  "description": "IDE (Integrated Development Environment) UI components.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "parcel src/html/index.html --port 1236",
    "watch": "yarn start",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "bundle": "sh script.sh/bundle.sh",
    "clean": "sh script.sh/clean.sh",
    "serve": "cd dist && serve",
    "copy.defs": "ts-node -T script.ts/copy.defs.ts",
    "prepare": "ts prepare"
  },
  "dependencies": {
    "@monaco-editor/react": "3.4.2",
    "@platform/cell.client": "0.2.161",
    "@platform/cell.schema.sys": "0.0.37",
    "@platform/cell.types": "0.5.1",
    "@platform/cell.typesystem": "0.0.119",
    "@platform/cell.ui": "0.1.127",
    "@platform/libs": "0.2.2",
    "@platform/react": "0.4.49",
    "@platform/state": "0.7.14",
    "@platform/types": "0.5.8",
    "@platform/ui.icon": "0.0.168",
    "monaco-themes": "0.3.3",
    "react-icons": "3.11.0"
  },
  "devDependencies": {
    "@platform/test": "0.1.16",
    "@platform/ts": "4.0.2",
    "@platform/ui.dev": "0.0.23"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
