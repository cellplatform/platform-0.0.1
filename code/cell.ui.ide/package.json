    "@platform/libs": "0.2.3",
    "@platform/react": "0.4.57",
    "@platform/state": "0.7.22",
    "@platform/types": "0.5.11",
    "@platform/ui.icon": "0.0.176",
    "monaco-themes": "0.3.3",
    "react-icons": "4.1.0"
  },
  "devDependencies": {
    "@platform/test": "0.2.0",
