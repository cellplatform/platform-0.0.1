{
  "name": "cell",
  "version": "1.0.8",
  "description": "Electron shell for CellOS.",
  "main": "lib/main/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "clean": "sh sh/clean.sh",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "bundle": "ts-node -T scripts/bundle.sys.ui",
    "open": "cd out/cell.electron-darwin-x64 && open cell.electron.app"
  },
  "config": {
    "forge": "forge.config.js"
  },
  "dependencies": {
    "@platform/cell.client": "0.1.33",
    "@platform/cell.fs": "0.3.69",
    "@platform/cell.http": "0.6.42",
    "@platform/cell.schema": "0.4.33",
    "@platform/cell.types": "0.4.28",
    "@platform/cell.ui.sys": "0.0.4",
    "@platform/fs": "0.5.5",
    "@platform/fsdb.nedb": "1.3.87",
    "@platform/libs": "0.2.0",
    "@platform/log": "0.1.9",
    "@platform/micro": "0.14.2",
    "@platform/util.value": "0.7.3",
    "electron-log": "4.0.6",
    "electron-squirrel-startup": "1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.49",
    "@electron-forge/maker-deb": "6.0.0-beta.49",
    "@electron-forge/maker-rpm": "6.0.0-beta.49",
    "@electron-forge/maker-squirrel": "6.0.0-beta.49",
    "@electron-forge/maker-zip": "6.0.0-beta.49",
    "@platform/exec": "0.2.45",
    "@platform/test": "0.1.2",
    "@platform/ts": "3.7.12",
    "electron": "8.0.1"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
