{
  "name": "app",
  "version": "0.1.40",
  "description": "Electron shell for CellOS.",
  "main": "lib/main/index",
  "types": "lib/index.d.ts",
  "config": {
    "forge": "build/forge.config.js"
  },
  "scripts": {
    "start": "yarn bundle.preload && electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "prepare": "ts prepare",
    "bundle.preload": "sh script.sh/bundle.preload.sh",
    "prepackage": "yarn build && yarn bundle.preload",
    "package": "yarn prepackage && electron-forge package",
    "make": "npm version patch && yarn prepackage && electron-forge make",
    "tmp": "electron-forge package"
  },
  "dependencies": {
    "@platform/cell.client": "0.2.163",
    "@platform/cell.fs": "0.4.128",
    "@platform/cell.fs.local": "0.4.128",
    "@platform/cell.http": "0.7.167",
    "@platform/cell.http.router": "0.8.4",
    "@platform/cell.schema": "0.5.6",
    "@platform/cell.schema.sys": "0.0.42",
    "@platform/cell.types": "0.5.7",
    "@platform/cell.typesystem": "0.0.125",
    "@platform/fs": "0.5.24",
    "@platform/fsdb.nedb": "1.3.137",
    "@platform/libs": "0.2.2",
    "@platform/log": "0.1.31",
    "@platform/types": "0.5.8",
    "@platform/util.value": "0.8.1",
    "electron-log": "4.3.0",
    "electron-squirrel-startup": "1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.54",
    "@electron-forge/maker-deb": "6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "6.0.0-beta.54",
    "@electron-forge/maker-zip": "6.0.0-beta.54",
    "@platform/exec": "0.2.63",
    "@platform/test": "0.1.16",
    "@platform/ts": "4.0.2",
    "browserify": "17.0.0",
    "electron": "10.1.5"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
