{
  "name": "app",
  "version": "0.1.54",
  "description": "Electron shell for CellOS.",
  "main": "lib/main/index",
  "types": "lib/index.d.ts",
  "config": {
    "forge": "build/forge.config.js"
  },
  "scripts": {
    "start": "yarn bundle.preload && electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "prepare": "ts prepare",
    "bundle.preload": "sh script.sh/bundle.preload.sh",
    "prepackage": "yarn build && yarn bundle.preload",
    "package": "export NOTARIZE=false && yarn prepackage && electron-forge package",
    "make": "npm version patch && yarn prepackage && electron-forge make"
  },
  "dependencies": {
    "@platform/cell.client": "0.2.172",
    "@platform/cell.fs": "0.4.134",
    "@platform/cell.fs.local": "0.4.134",
    "@platform/cell.http": "0.7.180",
    "@platform/cell.http.router": "0.8.17",
    "@platform/cell.schema": "0.5.12",
    "@platform/cell.schema.sys": "0.0.48",
    "@platform/cell.types": "0.5.13",
    "@platform/cell.typesystem": "0.0.131",
    "@platform/fs": "0.6.0",
    "@platform/fsdb.nedb": "1.3.141",
    "@platform/libs": "0.2.3",
    "@platform/log": "0.1.35",
    "@platform/types": "0.5.8",
    "@platform/util.value": "0.8.5",
    "electron-log": "4.3.0",
    "electron-squirrel-startup": "1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.54",
    "@electron-forge/maker-deb": "6.0.0-beta.54",
    "@electron-forge/maker-dmg": "6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "6.0.0-beta.54",
    "@electron-forge/maker-zip": "6.0.0-beta.54",
    "@platform/cell.compiler": "0.2.1",
    "@platform/exec": "0.2.67",
    "@platform/test": "0.1.17",
    "@platform/ts": "4.0.6",
    "browserify": "17.0.0",
    "electron": "10.1.5"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
