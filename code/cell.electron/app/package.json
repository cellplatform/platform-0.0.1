{
  "name": "app",
  "version": "1.0.70",
  "description": "Electron shell for CellOS.",
  "main": "lib/main/index",
  "types": "lib/index.d.ts",
  "config": {
    "forge": "forge.config.js"
  },
  "scripts": {
    "start": "yarn bundle.preload && electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "package": "electron-forge package",
    "make": "yarn build && electron-forge make",
    "bundle.preload": "sh script.sh/bundle.preload.sh"
  },
  "dependencies": {
    "@platform/cell.client": "0.2.104",
    "@platform/cell.fs": "0.4.67",
    "@platform/cell.fs.local": "0.4.67",
    "@platform/cell.http": "0.7.103",
    "@platform/cell.http.router": "0.7.101",
    "@platform/cell.schema": "0.4.123",
    "@platform/cell.types": "0.4.111",
    "@platform/cell.typesystem": "0.0.62",
    "@platform/fs": "0.5.17",
    "@platform/fsdb.nedb": "1.3.124",
    "@platform/libs": "0.2.2",
    "@platform/log": "0.1.25",
    "@platform/util.value": "0.7.24",
    "electron-log": "4.2.1",
    "electron-squirrel-startup": "1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.51",
    "@electron-forge/maker-deb": "6.0.0-beta.51",
    "@electron-forge/maker-rpm": "6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "6.0.0-beta.51",
    "@electron-forge/maker-zip": "6.0.0-beta.51",
    "@platform/exec": "0.2.59",
    "@platform/test": "0.1.12",
    "@platform/ts": "3.9.3",
    "electron": "9.0.3"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
