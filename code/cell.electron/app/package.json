{
  "name": "cell.app",
  "version": "1.0.20",
  "description": "Electron shell for CellOS.",
  "main": "lib/main/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "clean": "sh sh/clean.sh",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "bundle": "ts-node -T scripts/bundle.sys.ui"
  },
  "config": {
    "forge": "forge.config.js"
  },
  "dependencies": {
    "@platform/cell.client": "0.2.51",
    "@platform/cell.fs": "0.4.22",
    "@platform/cell.fs.local": "^0.4.22",
    "@platform/cell.http": "0.7.42",
    "@platform/cell.schema": "0.4.78",
    "@platform/cell.types": "0.4.70",
    "@platform/cell.typesystem": "0.0.15",
    "@platform/cell.ui.sys": "0.0.62",
    "@platform/fs": "0.5.12",
    "@platform/fsdb.nedb": "1.3.104",
    "@platform/libs": "0.2.1",
    "@platform/log": "0.1.17",
    "@platform/micro": "0.14.25",
    "@platform/util.value": "0.7.13",
    "electron-log": "4.1.1",
    "electron-squirrel-startup": "1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@platform/exec": "0.2.53",
    "@platform/test": "0.1.7",
    "@platform/ts": "3.7.20",
    "electron": "8.2.1"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
