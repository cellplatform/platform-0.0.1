{
  "name": "@platform/cell.electron",
  "version": "0.0.152",
  "description": "Electron shell for CellOS.",
  "scripts": {
    "ui": "cd app.ui && yarn start",
    "start": "cd app && yarn start",
    "dev": "cd app && yarn dev",
    "test": "cd app && yarn test",
    "package": "cd app && yarn package",
    "make": "yarn bundle && cd app && yarn make",
    "build": "cd app && yarn build",
    "clean": "sh script.sh/clean.sh",
    "reset": "sh script.sh/reset.sh",
    "nuke": "sh script.sh/nuke.sh",
    "bundle": "sh script.sh/bundle.ui.sys.sh",
    "open": "cd app/out/make && open ."
  },
  "devDependencies": {
    "@platform/cli.prompt": "0.3.30",
    "@platform/ts": "4.0.6"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
