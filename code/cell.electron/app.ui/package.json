{
  "name": "@platform/cell.electron.ui",
  "version": "0.0.0",
  "description": "UI components used by [cell.electron].",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "parcel src/index.html",
    "watch": "yarn start",
    "test": "ts test",
    "tdd": "ts test --watch",
