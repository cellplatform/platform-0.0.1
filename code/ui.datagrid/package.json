{
  "name": "@platform/ui.datagrid",
  "version": "0.7.220",
  "description": "Isolated tabular DataGrid.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "ui": "ui $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "build-test": "ts build --tsconfig=tsconfig.test $@",
    "prepare": "ts prepare",
    "start": "parcel --port=3000 test/parcel/index.html",
    "clean": "rm -rf .cache && rm -rf dist",
    "bundle": "yarn clean && parcel build test/parcel/index.html --no-source-maps --out-dir dist/parcel",
    "serve": "cd dist/parcel && serve"
  },
  "dependencies": {
    "@babel/polyfill": "7.8.7",
    "@platform/cache": "0.2.23",
    "@platform/cell.coord": "0.8.70",
    "@platform/cell.func": "0.1.110",
    "@platform/cell.schema": "0.4.58",
    "@platform/cell.types": "0.4.52",
    "@platform/cell.ui": "0.0.141",
    "@platform/cell.value": "0.2.20",
    "@platform/libs": "0.2.1",
    "@platform/react": "0.4.18",
    "@platform/ui.button": "0.3.51",
    "@platform/ui.datagrid.types": "0.7.112",
    "@platform/ui.datagrid.util": "0.2.97",
    "@platform/ui.text": "0.3.45",
    "handsontable": "6.2.2"
  },
  "devDependencies": {
    "@platform/cell.client": "0.2.30",
    "@platform/cell.func.sys": "0.2.71",
    "@platform/cell.http": "0.7.18",
    "@platform/cli.spec": "0.1.101",
    "@platform/polyfill": "0.0.1",
    "@platform/test": "0.1.7",
    "@platform/test.dom": "0.0.77",
    "@platform/ts": "3.7.20",
    "@platform/ui.datagrid.cell": "0.2.65",
    "@platform/ui.datagrid.debug": "0.0.103",
    "@platform/util.string": "0.4.11",
    "@uiharness/dev": "0.10.5",
    "@uiharness/ui": "0.7.5",
    "electron": "8.1.1"
  },
  "resolutions": {
    "prosemirror-state": "1.3.2"
  },
  "files": [
    "lib",
    "css",
    "import.css",
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
