    "@platform/cell.schema": "0.4.33",
    "@platform/cell.types": "0.4.28",
    "@platform/cell.ui": "0.0.117",
    "@platform/cell.value": "0.1.97",
    "@platform/libs": "0.2.0",
    "@platform/react": "0.4.8",
    "@platform/ui.button": "0.3.41",
    "@platform/ui.datagrid.types": "0.7.96",
    "@platform/ui.datagrid.util": "0.2.80",
    "@platform/ui.text": "0.3.35",
    "handsontable": "6.2.2"
  },
  "devDependencies": {
    "@platform/cell.client": "^0.1.33",
    "@platform/cell.func.sys": "0.2.47",
    "@platform/cell.http": "0.6.42",
    "@platform/cell.ui.props": "0.0.117",
    "@platform/cli.spec": "0.1.90",
    "@platform/polyfill": "0.0.1",
    "@platform/test": "0.1.2",
    "@platform/test.dom": "0.0.69",
    "@platform/ts": "3.7.12",
    "@platform/ui.datagrid.cell": "0.2.49",
    "@platform/ui.datagrid.debug": "0.0.86",
    "@platform/util.string": "^0.4.2",
    "@uiharness/dev": "0.10.4",
    "@uiharness/ui": "0.7.4",
    "electron": "7.1.10"
  },
  "resolutions": {
    "prosemirror-state": "1.3.2"
  },
  "files": [
    "lib",
    "css",
    "import.css",
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
