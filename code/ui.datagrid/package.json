{
  "name": "@platform/ui.datagrid",
  "version": "0.7.186",
  "description": "Isolated tabular DataGrid.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "ui": "ui $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "build-test": "ts build --tsconfig=tsconfig.test $@",
    "prepare": "ts prepare",
    "start": "parcel test/parcel/index.html",
    "clean": "rm -rf .cache && rm -rf dist/parcel",
    "bundle": "yarn clean && parcel build test/parcel/index.html --no-source-maps --out-dir dist/parcel",
    "serve": "cd dist/parcel && serve"
  },
  "dependencies": {
    "@babel/polyfill": "7.8.3",
    "@platform/cache": "0.2.15",
    "@platform/cell.coord": "0.8.40",
    "@platform/cell.func": "0.1.75",
    "@platform/cell.schema": "0.4.24",
    "@platform/cell.types": "0.4.22",
    "@platform/cell.ui": "0.0.111",
    "@platform/cell.value": "0.1.88",
    "@platform/libs": "0.2.0",
    "@platform/react": "0.4.8",
    "@platform/ui.button": "0.3.41",
    "@platform/ui.datagrid.types": "0.7.93",
    "@platform/ui.datagrid.util": "0.2.74",
    "@platform/ui.text": "0.3.35",
    "handsontable": "6.2.2"
  },
  "devDependencies": {
    "@platform/cell.client": "0.1.24",
    "@platform/cell.func.sys": "0.2.41",
    "@platform/cell.http": "0.6.29",
    "@platform/cell.ui.props": "0.0.110",
    "@platform/cli.spec": "0.1.87",
    "@platform/polyfill": "0.0.1",
    "@platform/test": "0.1.2",
    "@platform/test.dom": "0.0.69",
    "@platform/ts": "3.7.12",
    "@platform/ui.datagrid.cell": "0.2.46",
    "@platform/ui.datagrid.debug": "0.0.80",
    "@uiharness/dev": "0.10.4",
    "@uiharness/ui": "0.7.4",
    "electron": "7.1.10"
  },
  "resolutions": {
    "prosemirror-state": "1.3.2"
  },
  "files": [
    "lib",
    "css",
    "import.css",
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "repository": "uiharness/platform",
