{
  "name": "@platform/ui.datagrid",
  "version": "0.7.240",
  "description": "Isolated tabular DataGrid.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "ui": "ui $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "build-test": "ts build --tsconfig=tsconfig.test $@",
    "prepare": "ts prepare",
    "start": "parcel --port=3000 test/parcel/index.html",
    "clean": "rm -rf .cache && rm -rf dist",
    "bundle": "yarn clean && parcel build test/parcel/index.html --no-source-maps --out-dir dist/parcel",
    "serve": "cd dist/parcel && serve"
  },
  "dependencies": {
    "@babel/polyfill": "7.8.7",
    "@platform/cache": "0.2.31",
    "@platform/cell.coord": "0.8.119",
    "@platform/cell.func": "0.1.161",
    "@platform/cell.schema": "0.4.110",
    "@platform/cell.types": "0.4.100",
    "@platform/cell.ui": "0.1.18",
    "@platform/cell.value": "0.2.71",
    "@platform/libs": "0.2.2",
    "@platform/react": "0.4.28",
    "@platform/ui.button": "0.3.61",
    "@platform/ui.datagrid.types": "0.7.128",
    "@platform/ui.datagrid.util": "0.2.115",
    "@platform/ui.text": "0.3.55",
    "handsontable": "6.2.2"
  },
  "devDependencies": {
    "@platform/cell.client": "0.2.88",
    "@platform/cell.func.sys": "0.2.119",
    "@platform/cell.http": "0.7.80",
    "@platform/cli.spec": "0.1.111",
    "@platform/polyfill": "0.0.1",
    "@platform/test": "0.1.12",
    "@platform/test.dom": "0.0.85",
    "@platform/ts": "3.9.3",
    "@platform/ui.datagrid.cell": "0.2.81",
    "@platform/ui.datagrid.debug": "0.0.121",
    "@platform/util.string": "0.5.8",
    "@uiharness/dev": "0.10.5",
    "@uiharness/ui": "0.7.6",
    "electron": "9.0.0"
  },
  "resolutions": {
    "prosemirror-state": "1.3.2"
  },
  "files": [
    "lib",
    "css",
    "import.css",
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
