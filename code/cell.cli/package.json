{
  "name": "@platform/cell.cli",
  "version": "0.2.320",
  "description": "Command-line-interface for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/bin.js"
  },
  "scripts": {
    "demo": "mkdir -p tmp && cd tmp && node ../lib/bin $@",
    "start": "node lib/bin $@",
    "test": "ts test $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "clean": "rm -rf tmp",
    "prepare": "ts-node -T scripts.ts/prepare && ts prepare"
  },
  "dependencies": {
    "@platform/cell.fs.sync": "0.3.314",
    "@platform/cell.schema": "0.5.103",
    "@platform/cell.types": "0.5.100",
    "@platform/cli": "0.5.84",
    "@platform/cli.prompt": "0.3.57"
  },
  "devDependencies": {
    "@platform/npm": "0.4.18",
    "@platform/test": "0.2.10",
    "@platform/ts": "4.1.23"
  },
  "files": [
    "lib",
    "tmpl",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
