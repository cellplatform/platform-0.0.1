{
  "name": "cell.system",
  "version": "0.0.32",
  "scripts": {
    "now": "yarn vercel",
    "vercel": "vercel dev --listen 8080",
    "start": "ts-node -T src/env.node",
    "build": "ts build --no-esm $@",
    "dbuild": "sh sh/docker.build.sh",
    "drun": "docker run --rm -p 8080:5000 domains/cell",
    "dup": "docker-compose up",
    "dupb": "docker-compose up --build"
  },
  "dependencies": {
    "@platform/cell.fs.local": "^0.4.174",
    "@platform/cell.fs.s3": "^0.4.179",
    "@platform/cell.service": "^0.8.28",
    "@platform/cell.router": "^0.8.92",
    "@platform/cell.runtime.node": "^0.0.21",
    "@platform/cell.schema": "^0.5.52",
    "@platform/cell.types": "^0.5.52",
    "@platform/fsdb.mongo": "^0.0.157",
    "@platform/fsdb.nedb": "^1.3.151"
  },
  "devDependencies": {
    "@platform/ts": "^4.1.5"
  },
  "engines": {
    "node": ">=12.x"
  },
  "private": true
}
