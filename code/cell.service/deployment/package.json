{
  "name": "cell.system",
  "version": "0.0.22",
  "scripts": {
    "now": "yarn vercel",
    "vercel": "vercel dev --listen 8080",
    "start": "ts-node -T src/env.node",
    "build": "ts build --no-esm $@",
    "dbuild": "sh sh/docker.build.sh",
    "drun": "docker run --rm -p 8080:5000 domains/cell",
    "dup": "docker-compose up",
    "dupb": "docker-compose up --build"
  },
  "dependencies": {
    "@platform/cell.fs.local": "^0.4.165",
    "@platform/cell.fs.s3": "^0.4.170",
    "@platform/cell.service": "^0.8.18",
    "@platform/cell.router": "^0.8.82",
    "@platform/cell.runtime.node": "^0.0.12",
    "@platform/cell.schema": "^0.5.43",
    "@platform/cell.types": "^0.5.44",
    "@platform/fsdb.mongo": "^0.0.152",
    "@platform/fsdb.nedb": "^1.3.146"
  },
  "devDependencies": {
    "@platform/ts": "^4.1.0"
  },
  "engines": {
    "node": ">=12.x"
  },
  "private": true
}
