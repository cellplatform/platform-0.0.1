{
  "name": "cell.system",
  "version": "0.0.38",
  "scripts": {
    "now": "yarn vercel",
    "vercel": "vercel dev --listen 8080",
    "start": "ts-node -T src/env.node",
    "build": "ts build --no-esm $@",
    "dbuild": "sh sh/docker.build.sh",
    "drun": "docker run --rm -p 8080:5000 domains/cell",
    "dup": "docker-compose up",
    "dupb": "docker-compose up --build"
  },
  "dependencies": {
    "@platform/cell.fs.local": "^0.4.183",
    "@platform/cell.fs.s3": "^0.4.188",
    "@platform/cell.service": "^0.8.40",
    "@platform/cell.router": "0.9.5",
    "@platform/cell.runtime.node": "^0.0.30",
    "@platform/cell.schema": "^0.5.61",
    "@platform/cell.types": "^0.5.60",
    "@platform/fsdb.mongo": "^0.0.162",
    "@platform/fsdb.nedb": "^1.3.156",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@platform/ts": "^4.1.9"
  },
  "engines": {
    "node": ">=12.x"
  },
  "private": true
}
