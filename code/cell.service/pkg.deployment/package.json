{
  "name": "cell.system",
  "version": "0.0.40",
  "scripts": {
    "now": "yarn vercel",
    "vercel": "vercel dev --listen 8080",
    "start": "ts-node -T src/env.node",
    "build": "ts build --no-esm $@",
    "dbuild": "sh sh/docker.build.sh",
    "drun": "docker run --rm -p 8080:5000 domains/cell",
    "dup": "docker-compose up",
    "dupb": "docker-compose up --build"
  },
  "dependencies": {
    "@platform/cell.fs.local": "^0.4.185",
    "@platform/cell.fs.s3": "^0.4.190",
    "@platform/cell.service": "^0.8.42",
    "@platform/cell.router": "0.9.7",
    "@platform/cell.runtime.node": "^0.0.32",
    "@platform/cell.schema": "^0.5.63",
    "@platform/cell.types": "^0.5.62",
    "@platform/fsdb.mongo": "^0.0.164",
    "@platform/fsdb.nedb": "^1.3.158",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@platform/ts": "^4.1.11"
  },
  "engines": {
    "node": ">=12.x"
  },
  "private": true
}
