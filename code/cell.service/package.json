{
  "name": "@platform/cell.service",
  "version": "0.8.40",
  "description": "HTTP micro-service for the operating system.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd pkg.deployment && yarn start $@",
    "dev": "export NODE_ENV=development && yarn start",
    "watch": "cd pkg.deployment && yarn watch $@",
    "now": "cd pkg.deployment && yarn vercel $@",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "deploy": "sh script.sh/deploy.sh",
    "prepare": "ts-node -T script.ts/prepare && ts prepare && rm -rf tmp",
    "reset": "ts-node -T script.ts/reset",
    "nuke": "sh script.sh/nuke.sh",
    "dbuild": "cd pkg.deployment && yarn dbuild",
    "public": "ngrok http 8080 --hostname=os.ngrok.io"
  },
  "dependencies": {
    "@platform/cell.router": "0.9.5",
    "@platform/cell.schema": "0.5.61",
    "@platform/cell.types": "0.5.60",
    "@platform/cell.typesystem": "0.0.180",
    "@platform/cli": "0.5.68",
    "@platform/exec": "0.2.79",
    "@platform/fs": "0.6.10",
    "@platform/http": "0.14.13",
    "@platform/libs": "0.2.9",
    "@platform/micro": "0.16.9",
    "@platform/types": "0.5.18",
    "@platform/util.value": "0.8.18"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.156",
    "@platform/npm": "0.4.2",
    "@platform/test": "0.2.5",
    "@platform/ts": "4.1.9"
  },
  "files": [
    "lib",
    "static",
    "!lib/test/",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
