{
  "name": "@platform/cell.service",
  "version": "0.8.28",
  "description": "HTTP micro-service for the operating system.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd deployment && yarn start $@",
    "dev": "export NODE_ENV=development && yarn start",
    "watch": "cd deployment && yarn watch $@",
    "now": "cd deployment && yarn vercel $@",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "deploy": "yarn cell deploy $@",
    "prepare": "ts-node -T script.ts/prepare && ts prepare && rm -rf tmp",
    "reset": "ts-node -T script.ts/reset",
    "nuke": "rm -rf node_modules && rm -rf deployment/node_modules",
    "dbuild": "cd deployment && yarn dbuild"
  },
  "dependencies": {
    "@platform/cell.router": "0.8.92",
    "@platform/cell.schema": "0.5.52",
    "@platform/cell.types": "0.5.52",
    "@platform/cell.typesystem": "0.0.171",
    "@platform/cli": "0.5.64",
    "@platform/exec": "0.2.75",
    "@platform/fs": "0.6.6",
    "@platform/http": "0.14.9",
    "@platform/libs": "0.2.6",
    "@platform/micro": "0.16.5",
    "@platform/types": "0.5.14",
    "@platform/util.value": "0.8.14"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.151",
    "@platform/npm": "0.3.27",
    "@platform/test": "0.2.2",
    "@platform/ts": "4.1.5"
  },
  "files": [
    "lib",
    "static",
    "!lib/test/",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
