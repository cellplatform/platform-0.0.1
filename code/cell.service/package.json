{
  "name": "@platform/cell.service",
  "version": "0.8.0",
  "description": "HTTP micro-service for the operating system.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.deployment && yarn start $@",
    "dev": "export NODE_ENV=development && yarn start",
    "watch": "cd src.deployment && yarn watch $@",
    "now": "cd src.deployment && yarn vercel $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "deploy": "yarn cell deploy $@",
    "prepare": "ts-node -T script.ts/prepare && ts prepare && rm -rf tmp",
    "reset": "ts-node -T script.ts/reset",
    "nuke": "rm -rf node_modules && rm -rf src.deployment/node_modules"
  },
  "dependencies": {
    "@platform/cell.router": "0.8.65",
    "@platform/cell.schema": "0.5.30",
    "@platform/cell.types": "0.5.31",
    "@platform/cell.typesystem": "0.0.149",
    "@platform/cli": "0.5.59",
    "@platform/exec": "0.2.70",
    "@platform/fs": "0.6.3",
    "@platform/http": "0.14.4",
    "@platform/libs": "0.2.3",
    "@platform/micro": "0.16.0",
    "@platform/types": "0.5.11",
    "@platform/util.value": "0.8.9"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.146",
    "@platform/npm": "0.3.22",
    "@platform/test": "0.2.0",
    "@platform/ts": "4.1.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test/",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
