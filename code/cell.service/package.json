{
  "name": "@platform/cell.service",
  "version": "0.8.34",
  "description": "HTTP micro-service for the operating system.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd deployment && yarn start $@",
    "dev": "export NODE_ENV=development && yarn start",
    "watch": "cd deployment && yarn watch $@",
    "now": "cd deployment && yarn vercel $@",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "deploy": "yarn cell deploy $@",
    "prepare": "ts-node -T script.ts/prepare && ts prepare && rm -rf tmp",
    "reset": "ts-node -T script.ts/reset",
    "nuke": "sh script.sh/nuke.sh",
    "dbuild": "cd deployment && yarn dbuild"
  },
  "dependencies": {
    "@platform/cell.router": "0.8.98",
    "@platform/cell.schema": "0.5.58",
    "@platform/cell.types": "0.5.57",
    "@platform/cell.typesystem": "0.0.177",
    "@platform/cli": "0.5.66",
    "@platform/exec": "0.2.77",
    "@platform/fs": "0.6.8",
    "@platform/http": "0.14.11",
    "@platform/libs": "0.2.7",
    "@platform/micro": "0.16.7",
    "@platform/types": "0.5.16",
    "@platform/util.value": "0.8.16"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.153",
    "@platform/npm": "0.4.0",
    "@platform/test": "0.2.3",
    "@platform/ts": "4.1.7"
  },
  "files": [
    "lib",
    "static",
    "!lib/test/",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
