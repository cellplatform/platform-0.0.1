{
  "name": "cell.runtime.electron",
  "version": "0.44.10",
  "description": "Desktop runtime environment.",
  "main": "lib/main/index",
  "types": "lib/index.d.ts",
  "config": {
    "forge": "build/forge.config.js"
  },
  "scripts": {
    "start": "yarn bundle.preload && electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "bundle.preload": "sh script.sh/bundle.preload.sh",
    "prepackage": "yarn build && yarn bundle.preload",
    "package": "export NOTARIZE=false && yarn prepackage && electron-forge package",
    "make": "yarn prepackage && electron-forge make $@"
  },
  "dependencies": {
    "@platform/cell.client": "0.3.88",
    "@platform/cell.fs": "0.4.220",
    "@platform/cell.fs.local": "0.4.220",
    "@platform/cell.router": "0.9.42",
    "@platform/cell.runtime.node": "0.0.67",
    "@platform/cell.schema": "0.5.98",
    "@platform/cell.schema.sys": "0.0.107",
    "@platform/cell.service": "0.8.80",
    "@platform/cell.types": "0.5.95",
    "@platform/cell.typesystem": "0.0.217",
    "@platform/fs": "0.7.3",
    "@platform/fsdb.nedb": "1.3.170",
    "@platform/libs": "0.2.12",
    "@platform/log": "0.1.60",
    "@platform/state": "0.7.50",
    "@platform/state.types": "0.1.13",
    "@platform/types": "0.5.23",
    "@platform/util.value": "0.9.8",
    "electron-log": "4.3.5",
    "semver": "^7.3.5",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.58",
    "@electron-forge/maker-deb": "6.0.0-beta.58",
    "@electron-forge/maker-dmg": "6.0.0-beta.58",
    "@electron-forge/maker-rpm": "6.0.0-beta.58",
    "@electron-forge/maker-squirrel": "6.0.0-beta.58",
    "@electron-forge/maker-zip": "6.0.0-beta.58",
    "@platform/exec": "0.3.10",
    "@platform/test": "0.2.8",
    "@platform/ts": "4.1.21",
    "browserify": "17.0.0",
    "electron": "13.1.7",
    "electron-squirrel-startup": "1.0.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
