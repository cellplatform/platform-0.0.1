{
  "name": "cell.runtime.electron",
  "version": "0.55.4",
  "description": "Desktop runtime environment.",
  "main": "lib/main/index.js",
  "types": "lib/index.d.ts",
  "config": {
    "forge": "build/forge.config.js"
  },
  "scripts": {
    "start": "yarn bundle.preload && electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "bundle.preload": "sh script.sh/bundle.preload.sh",
    "prepackage": "yarn build && yarn bundle.preload",
    "package": "export NOTARIZE=false && yarn prepackage && electron-forge package",
    "make": "yarn prepackage && electron-forge make $@"
  },
  "dependencies": {
    "@platform/cell.client": "0.3.92",
    "@platform/cell.fs": "0.4.224",
    "@platform/cell.fs.local": "0.4.224",
    "@platform/cell.fs.bus": "0.0.7",
    "@platform/cell.router": "0.9.46",
    "@platform/cell.runtime.node": "0.0.71",
    "@platform/cell.schema": "0.5.102",
    "@platform/cell.schema.sys": "0.0.107",
    "@platform/cell.service": "0.8.84",
    "@platform/cell.types": "0.5.99",
    "@platform/cell.typesystem": "0.0.221",
    "@platform/fs": "0.7.5",
    "@platform/fsdb.nedb": "1.3.172",
    "@platform/libs": "0.2.14",
    "@platform/log": "0.1.62",
    "@platform/state": "0.7.52",
    "@platform/state.types": "0.1.15",
    "@platform/types": "0.5.25",
    "@platform/util.value": "0.9.10",
    "electron-log": "4.4.1",
    "semver": "7.3.5",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.60",
    "@electron-forge/maker-deb": "6.0.0-beta.60",
    "@electron-forge/maker-dmg": "6.0.0-beta.60",
    "@electron-forge/maker-rpm": "6.0.0-beta.60",
    "@electron-forge/maker-squirrel": "6.0.0-beta.60",
    "@electron-forge/maker-zip": "6.0.0-beta.60",
    "@platform/exec": "0.3.12",
    "@platform/test": "0.2.10",
    "@platform/ts": "4.1.23",
    "browserify": "17.0.0",
    "electron": "14.0.0",
    "electron-squirrel-startup": "1.0.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
