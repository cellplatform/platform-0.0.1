{
  "name": "@platform/cell.runtime.electron",
  "version": "0.0.178",
  "description": "Electron shell for CellOS.",
  "scripts": {
    "start": "cd app.electron && yarn start",
    "dev": "cd app.electron && yarn dev",
    "test": "cd app.electron && yarn test",
    "package": "cd app.electron && yarn package",
    "make": "cd app.electron && yarn make",
    "build": "cd app.electron && yarn build",
    "open": "cd app.electron/out/make && open .",
    "clean": "sh script.sh/clean.sh",
    "reset": "sh script.sh/reset.sh",
    "nuke": "sh script.sh/nuke.sh",
    "public": "ngrok http 5000 --hostname=os.ngrok.io"
  },
  "devDependencies": {
    "@platform/cell.runtime": "0.10.22",
    "@platform/cli.prompt": "0.3.45",
    "@platform/ts": "4.1.11"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
