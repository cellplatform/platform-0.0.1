{
  "name": "@platform/cell.runtime.electron",
  "version": "0.0.178",
  "description": "Runtime for desktop environments",
  "scripts": {
    "start": "cd app && yarn start",
    "dev": "cd app && yarn dev",
    "test": "cd app && yarn test",
    "package": "cd app && yarn package",
    "make": "yarn cmd prepare make && cd app && yarn make",
    "build": "cd app && yarn build",
    "open": "cd app/out/make && open .",
    "clean": "sh script.sh/clean.sh",
    "reset": "sh script.sh/reset.sh",
    "nuke": "sh script.sh/nuke.sh",
    "public": "ngrok http 5000 --hostname=os.ngrok.io",
    "cmd": "ts-node -T node.bin $@"
  },
  "devDependencies": {
    "@platform/ts": "4.1.11",
    "@platform/cell.compiler": "0.3.20",
    "@types/minimist": "1.2.1",
    "minimist": "1.2.5",
    "ora": "5.4.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
