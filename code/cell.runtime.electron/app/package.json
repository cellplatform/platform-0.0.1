{
  "name": "cell.runtime.electron",
  "version": "0.11.3",
  "description": "Desktop runtime environment.",
  "main": "lib/main/index",
  "types": "lib/index.d.ts",
  "config": {
    "forge": "build/forge.config.js"
  },
  "scripts": {
    "start": "yarn bundle.preload && electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "bundle.preload": "sh script.sh/bundle.preload.sh",
    "prepackage": "yarn build && yarn bundle.preload",
    "package": "export NOTARIZE=false && yarn prepackage && electron-forge package",
    "make": "yarn prepackage && electron-forge make",
    "open": "cd out/make && open ."
  },
  "dependencies": {
    "@platform/cell.client": "0.3.63",
    "@platform/cell.fs": "0.4.195",
    "@platform/cell.fs.local": "0.4.195",
    "@platform/cell.router": "0.9.17",
    "@platform/cell.schema": "0.5.73",
    "@platform/cell.schema.sys": "0.0.107",
    "@platform/cell.service": "0.8.54",
    "@platform/cell.types": "0.5.70",
    "@platform/cell.typesystem": "0.0.192",
    "@platform/cell.runtime.node": "0.0.42",
    "@platform/fs": "0.6.13",
    "@platform/fsdb.nedb": "1.3.162",
    "@platform/libs": "0.2.10",
    "@platform/log": "0.1.52",
    "@platform/state": "0.7.42",
    "@platform/state.types": "0.1.5",
    "@platform/types": "0.5.21",
    "@platform/util.value": "0.9.0",
    "electron-log": "4.3.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.54",
    "@electron-forge/maker-deb": "6.0.0-beta.54",
    "@electron-forge/maker-dmg": "6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "6.0.0-beta.54",
    "@electron-forge/maker-zip": "6.0.0-beta.54",
    "@platform/exec": "0.3.2",
    "@platform/test": "0.2.6",
    "@platform/ts": "4.1.13",
    "browserify": "17.0.0",
    "electron": "12.0.7",
    "electron-squirrel-startup": "1.0.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "*"
    ]
  },
  "resolutions": {
    "rxjs": "7.0.1",
    "core-js": "3.12.1"
  }
}
