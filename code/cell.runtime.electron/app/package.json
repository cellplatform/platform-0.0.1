{
  "name": "app",
  "version": "0.1.140",
  "description": "Electron shell for CellOS.",
  "main": "lib/main/index",
  "types": "lib/index.d.ts",
  "config": {
    "forge": "build/forge.config.js"
  },
  "workspaces__": {
    "packages": [
      "*"
    ]
  },
  "scripts": {
    "start": "yarn bundle.preload && electron-forge start",
    "dev": "export NODE_ENV=development && yarn start",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "bundle.preload": "sh script.sh/bundle.preload.sh",
    "prepackage": "yarn build && yarn bundle.preload",
    "package": "export NOTARIZE=false && yarn prepackage && electron-forge package",
    "make": "npm version patch && yarn prepackage && electron-forge make"
  },
  "dependencies": {
    "@platform/cell.client": "0.3.42",
    "@platform/cell.fs": "0.4.174",
    "@platform/cell.fs.local": "0.4.174",
    "@platform/cell.router": "0.8.92",
    "@platform/cell.schema": "0.5.52",
    "@platform/cell.schema.sys": "0.0.91",
    "@platform/cell.service": "0.8.28",
    "@platform/cell.types": "0.5.52",
    "@platform/cell.typesystem": "0.0.171",
    "@platform/fs": "0.6.6",
    "@platform/fsdb.nedb": "1.3.151",
    "@platform/libs": "0.2.6",
    "@platform/log": "0.1.44",
    "@platform/types": "0.5.14",
    "@platform/util.value": "0.8.14",
    "electron-log": "4.3.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.54",
    "@electron-forge/maker-deb": "6.0.0-beta.54",
    "@electron-forge/maker-dmg": "6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "6.0.0-beta.54",
    "@electron-forge/maker-zip": "6.0.0-beta.54",
    "@platform/exec": "0.2.75",
    "@platform/test": "0.2.2",
    "@platform/ts": "4.1.5",
    "browserify": "17.0.0",
    "electron": "11.1.0",
    "electron-squirrel-startup": "1.0.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
