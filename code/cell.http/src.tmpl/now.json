{
  "version": 2,
  "name": "name",
  "alias": "domain.com",
  "env": {
    "CELL_MONGO": "@platform-mongo",
    "CELL_S3_KEY": "@spaces-key",
    "CELL_S3_SECRET": "@spaces-secret"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/now.ts"
    }
  ],
  "builds": [
    {
      "src": "**/*.{ts,tsx}",
      "use": "@now/node",
      "config": {
        "includeFiles": ["*.json", "static/favicon.ico"]
      }
    }
  ]
}
