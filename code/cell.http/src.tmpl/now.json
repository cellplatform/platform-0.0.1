{
  "version": 2,
  "name": "name",
  "alias": "domain.com",
  "env": {
    "CELL_MONGO": "@platform-mongo"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/now.ts"
    }
  ],
  "builds": [
    {
      "src": "**/*.{ts,tsx}",
      "use": "@now/node",
      "config": {
        "includeFiles": ["*.json"]
      }
    }
  ]
}
