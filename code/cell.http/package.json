{
  "name": "@platform/cell.http",
  "version": "0.6.19",
  "description": "HTTP cloud server, and client tools, for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "reset": "ts-node -T scripts/reset",
    "deploy": "yarn cell deploy $@"
  },
  "dependencies": {
    "@platform/cell.client": "0.1.23",
    "@platform/cell.db": "0.2.103",
    "@platform/cell.fs": "0.3.57",
    "@platform/cell.func": "0.1.74",
    "@platform/cell.schema": "0.4.23",
    "@platform/cell.types": "0.4.21",
    "@platform/cell.value": "0.1.87",
    "@platform/cli": "0.5.15",
    "@platform/cli.prompt": "0.3.2",
    "@platform/exec": "0.2.45",
    "@platform/fs": "0.5.5",
    "@platform/fsdb.model": "0.6.30",
    "@platform/fsdb.types": "0.6.8",
    "@platform/http": "0.11.1",
    "@platform/libs": "0.2.0",
    "@platform/micro": "0.12.37",
    "@platform/types": "0.4.4",
    "@platform/util.value": "0.7.3",
    "cheerio": "1.0.0-rc.3",
    "mime-types": "2.1.26"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.85",
    "@platform/test": "0.1.2",
    "@platform/ts": "3.7.12",
    "@types/cheerio": "0.22.15",
    "@types/form-data": "2.5.0",
    "@types/mime-types": "2.1.0",
    "form-data": "3.0.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}",
    "src.tmpl",
    "!src.tmpl/node_modules",
    "!src.tmpl/tmp"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
