{
  "name": "@platform/cell.http",
  "version": "0.7.235",
  "description": "HTTP server for the Cell operating system.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.deployment && yarn start $@",
    "dev": "export NODE_ENV=development && yarn start",
    "watch": "cd src.deployment && yarn watch $@",
    "now": "cd src.deployment && yarn vercel $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "deploy": "yarn cell deploy $@",
    "prepare": "ts-node -T script.ts/prepare && ts prepare && rm -rf tmp",
    "reset": "ts-node -T script.ts/reset",
    "nuke": "rm -rf node_modules && rm -rf src.deployment/node_modules",
    "dup": "cd docker && yarn dup"
  },
  "dependencies": {
    "@platform/cell.http.router": "0.8.58",
    "@platform/cell.schema": "0.5.24",
    "@platform/cell.types": "0.5.25",
    "@platform/cell.typesystem": "0.0.143",
    "@platform/cli": "0.5.55",
    "@platform/exec": "0.2.67",
    "@platform/fs": "0.6.0",
    "@platform/http": "0.14.0",
    "@platform/libs": "0.2.3",
    "@platform/micro": "0.15.2",
    "@platform/types": "0.5.8",
    "@platform/util.value": "0.8.5"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.142",
    "@platform/npm": "0.3.18",
    "@platform/test": "0.1.17",
    "@platform/ts": "4.0.6"
  },
  "files": [
    "lib",
    "static",
    "!lib/test/",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
