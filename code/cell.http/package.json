{
  "name": "@platform/cell.http",
  "version": "0.4.21",
  "description": "HTTP cloud server, and client tools, for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "reset": "ts-node -T scripts/reset",
    "deploy": "yarn cell deploy $@"
  },
  "dependencies": {
    "@platform/cell.db": "0.2.8",
    "@platform/cell.fs": "0.1.10",
    "@platform/cell.func": "0.1.5",
    "@platform/cell.schema": "0.1.12",
    "@platform/cell.types": "0.2.4",
    "@platform/cell.value": "0.1.18",
    "@platform/cli": "0.4.13",
    "@platform/cli.prompt": "0.2.0",
    "@platform/exec": "0.2.35",
    "@platform/fs": "0.3.5",
    "@platform/fsdb.model": "0.5.3",
    "@platform/fsdb.types": "0.5.6",
    "@platform/http": "0.5.1",
    "@platform/libs": "0.1.5",
    "@platform/micro": "0.10.0",
    "@platform/types": "0.2.0",
    "@types/minimist": "1.2.0",
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.40",
    "@platform/test": "0.0.66",
    "@platform/ts": "3.7.2",
    "@types/form-data": "2.5.0",
    "form-data": "3.0.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}",
    "src.tmpl",
    "!src.tmpl/node_modules"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
