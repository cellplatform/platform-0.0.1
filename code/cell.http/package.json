{
  "name": "@platform/cell.http",
  "version": "0.5.27",
  "description": "HTTP cloud server, and client tools, for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "echo ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "reset": "ts-node -T scripts/reset",
    "deploy": "yarn cell deploy $@"
  },
  "dependencies": {
    "@platform/cell.client": "0.0.47",
    "@platform/cell.db": "0.2.59",
    "@platform/cell.fs": "0.3.22",
    "@platform/cell.func": "0.1.42",
    "@platform/cell.schema": "0.3.13",
    "@platform/cell.types": "0.3.16",
    "@platform/cell.value": "0.1.55",
    "@platform/cli": "0.4.23",
    "@platform/cli.prompt": "0.2.5",
    "@platform/exec": "0.2.40",
    "@platform/fs": "0.5.1",
    "@platform/fsdb.model": "0.6.7",
    "@platform/fsdb.types": "0.6.0",
    "@platform/http": "0.9.5",
    "@platform/libs": "0.1.5",
    "@platform/micro": "0.12.21",
    "@platform/types": "0.3.3"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.62",
    "@platform/test": "0.1.0",
    "@platform/ts": "3.7.7",
    "@types/form-data": "2.5.0",
    "form-data": "3.0.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}",
    "src.tmpl",
    "!src.tmpl/node_modules",
    "!src.tmpl/tmp"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
