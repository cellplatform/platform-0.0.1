{
  "name": "@platform/cell.http",
  "version": "0.7.171",
  "description": "HTTP server for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "dev": "export NODE_ENV=development && yarn start",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "reset": "ts-node -T script.ts/reset",
    "deploy": "yarn cell deploy $@",
    "prepare": "ts-node -T script.ts/prepare && ts prepare && rm -rf tmp"
  },
  "dependencies": {
    "@platform/cell.http.router": "0.8.8",
    "@platform/cell.schema": "0.5.10",
    "@platform/cell.types": "0.5.11",
    "@platform/cell.typesystem": "0.0.129",
    "@platform/cli": "0.5.55",
    "@platform/exec": "0.2.67",
    "@platform/fs": "0.6.0",
    "@platform/http": "0.12.36",
    "@platform/libs": "0.2.3",
    "@platform/micro": "0.14.56",
    "@platform/types": "0.5.8",
    "@platform/util.value": "0.8.5"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.141",
    "@platform/npm": "0.3.18",
    "@platform/test": "0.1.16",
    "@platform/ts": "4.0.6"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "src.tmpl",
    "!src.tmpl/node_modules",
    "!src.tmpl/tmp"
  ],
  "license": "MIT"
}
