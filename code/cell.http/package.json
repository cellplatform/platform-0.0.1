{
  "name": "@platform/cell.http",
  "version": "0.7.134",
  "description": "HTTP server for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "dev": "export NODE_ENV=development && yarn start",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "reset": "ts-node -T script.ts/reset",
    "deploy": "yarn cell deploy $@",
    "prepare": "ts-node -T script.ts/prepare && ts prepare && rm -rf tmp"
  },
  "dependencies": {
    "@platform/cell.http.router": "0.7.132",
    "@platform/cell.schema": "0.4.152",
    "@platform/cell.types": "0.4.138",
    "@platform/cell.typesystem": "0.0.92",
    "@platform/cli": "0.5.45",
    "@platform/exec": "0.2.61",
    "@platform/fs": "0.5.19",
    "@platform/http": "0.12.26",
    "@platform/libs": "0.2.2",
    "@platform/micro": "0.14.46",
    "@platform/types": "0.5.3",
    "@platform/util.value": "0.7.30"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.131",
    "@platform/npm": "0.3.8",
    "@platform/test": "0.1.14",
    "@platform/ts": "3.9.5"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "src.tmpl",
    "!src.tmpl/node_modules",
    "!src.tmpl/tmp"
  ],
  "license": "MIT"
}
