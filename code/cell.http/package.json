{
  "name": "@platform/cell.http",
  "version": "0.7.7",
  "description": "HTTP server for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "reset": "ts-node -T scripts/reset",
    "deploy": "yarn cell deploy $@"
  },
  "dependencies": {
    "@platform/cell.http.router": "0.7.5",
    "@platform/cell.schema": "0.4.52",
    "@platform/cli": "0.5.25",
    "@platform/exec": "0.2.50",
    "@platform/fs": "0.5.10",
    "@platform/http": "0.11.8",
    "@platform/libs": "0.2.0",
    "@platform/micro": "0.14.12",
    "@platform/types": "0.4.11",
    "@platform/util.value": "0.7.10"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.94",
    "@platform/test": "0.1.5",
    "@platform/ts": "3.7.17"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}",
    "src.tmpl",
    "!src.tmpl/node_modules",
    "!src.tmpl/tmp"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
