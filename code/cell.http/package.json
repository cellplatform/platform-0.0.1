{
  "name": "@platform/cell.http",
  "version": "0.7.80",
  "description": "HTTP server for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "dev": "export NODE_ENV=development && yarn start",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare && rm -rf tmp",
    "reset": "ts-node -T scripts/reset",
    "deploy": "yarn cell deploy $@"
  },
  "dependencies": {
    "@platform/cell.http.router": "0.7.78",
    "@platform/cell.schema": "0.4.110",
    "@platform/cell.typesystem": "0.0.49",
    "@platform/cli": "0.5.37",
    "@platform/exec": "0.2.59",
    "@platform/fs": "0.5.17",
    "@platform/http": "0.12.15",
    "@platform/libs": "0.2.2",
    "@platform/micro": "0.14.35",
    "@platform/types": "0.4.22",
    "@platform/util.value": "0.7.22"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.115",
    "@platform/npm": "0.3.0",
    "@platform/test": "0.1.12",
    "@platform/ts": "3.9.3"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}",
    "src.tmpl",
    "!src.tmpl/node_modules",
    "!src.tmpl/tmp"
  ],
  "license": "MIT"
}
