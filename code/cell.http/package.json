{
  "name": "@platform/cell.http",
  "version": "0.6.64",
  "description": "HTTP cloud server, and client tools, for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "reset": "ts-node -T scripts/reset",
    "deploy": "yarn cell deploy $@",
    "logs": "now logs dev.db.team"
  },
  "dependencies": {
    "@platform/cache": "^0.2.20",
    "@platform/cell.client": "0.2.16",
    "@platform/cell.db": "0.3.11",
    "@platform/cell.fs": "0.3.86",
    "@platform/cell.func": "0.1.101",
    "@platform/cell.schema": "^0.4.49",
    "@platform/cell.types": "0.4.43",
    "@platform/cell.value": "0.2.11",
    "@platform/cli": "0.5.25",
    "@platform/cli.prompt": "0.3.9",
    "@platform/exec": "0.2.50",
    "@platform/fs": "0.5.10",
    "@platform/fsdb.model": "0.6.39",
    "@platform/fsdb.types": "0.6.15",
    "@platform/http": "0.11.8",
    "@platform/libs": "0.2.0",
    "@platform/micro": "0.14.12",
    "@platform/types": "^0.4.11",
    "@platform/util.value": "0.7.10",
    "cheerio": "1.0.0-rc.3",
    "mime-types": "2.1.26"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.94",
    "@platform/test": "0.1.5",
    "@platform/ts": "3.7.17",
    "@types/cheerio": "0.22.16",
    "@types/form-data": "2.5.0",
    "@types/mime-types": "2.1.0",
    "form-data": "3.0.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}",
    "src.tmpl",
    "!src.tmpl/node_modules",
    "!src.tmpl/tmp"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
