{
  "name": "@platform/cell.http",
  "version": "0.3.3",
  "description": "HTTP cloud server, and client tools, for CellOS.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell": "lib/cli/bin.js"
  },
  "scripts": {
    "cell": "node lib/cli/bin $@",
    "start": "cd src.tmpl && yarn start $@",
    "watch": "cd src.tmpl && yarn watch $@",
    "now": "cd src.tmpl && yarn now $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "reset": "ts-node -T scripts/reset",
    "deploy": "ts-node -T scripts/deploy $@"
  },
  "dependencies": {
    "@platform/cell.db": "0.1.8",
    "@platform/cell.func": "0.0.47",
    "@platform/cell.schema": "0.0.20",
    "@platform/cell.types": "0.1.14",
    "@platform/cell.value": "0.1.6",
    "@platform/cli": "0.4.9",
    "@platform/cli.prompt": "^0.1.0",
    "@platform/exec": "0.2.35",
    "@platform/fs": "0.3.5",
    "@platform/fsdb.model": "0.5.0",
    "@platform/fsdb.types": "0.5.5",
    "@platform/http": "0.4.19",
    "@platform/libs": "0.1.5",
    "@platform/micro": "0.8.5",
    "@platform/types": "0.1.1",
    "@types/minimist": "1.2.0",
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "@platform/fsdb.nedb": "1.3.37",
    "@platform/test": "0.0.66",
    "@platform/ts": "3.7.2"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}",
    "src.tmpl"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
