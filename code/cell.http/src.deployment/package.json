{
  "name": "cell.system",
  "version": "0.0.5",
  "scripts": {
    "now": "yarn vercel",
    "vercel": "vercel dev --listen 8080",
    "start": "ts-node -T src/env.node",
    "build": "ts build --no-esm $@",
    "dbuild": "yarn build && docker build -t domains/cell .",
    "dpush": "docker push domains/cell",
    "drun": "docker run --rm -p 8080:8080 domains/cell",
    "dup": "docker-compose up",
    "dupb": "docker-compose up --build"
  },
  "dependencies": {
    "@platform/cell.fs.local": "^0.4.152",
    "@platform/cell.fs.s3": "^0.4.157",
    "@platform/cell.http": "^0.7.242",
    "@platform/cell.router": "^0.8.65",
    "@platform/cell.runtime": "^0.0.17",
    "@platform/cell.schema": "^0.5.30",
    "@platform/cell.types": "^0.5.31",
    "@platform/fsdb.mongo": "^0.0.152",
    "@platform/fsdb.nedb": "^1.3.146"
  },
  "devDependencies": {
    "@platform/ts": "^4.1.0"
  },
  "engines": {
    "node": ">=12.x"
  },
  "private": true
}
