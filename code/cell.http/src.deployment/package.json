{
  "name": "cell.system",
  "version": "0.0.0",
  "scripts": {
    "now": "yarn vercel",
    "vercel": "vercel dev --listen 8080",
    "start": "ts-node -T src/env.node",
    "build": "ts build --no-esm $@",
    "dbuild": "yarn build && docker build -t domains/cell .",
    "drun": "docker run --rm -p 8080:8080 domains/cell",
    "dup": "docker-compose up",
    "dupb": "docker-compose up --build"
  },
  "dependencies": {
    "@platform/cell.fs.local": "^0.4.148",
    "@platform/cell.fs.s3": "^0.4.153",
    "@platform/cell.http": "^0.7.237",
    "@platform/cell.router": "^0.8.60",
    "@platform/cell.runtime": "^0.0.13",
    "@platform/cell.schema": "^0.5.26",
    "@platform/cell.types": "^0.5.27",
    "@platform/fsdb.mongo": "^0.0.148",
    "@platform/fsdb.nedb": "^1.3.142"
  },
  "devDependencies": {
    "@platform/ts": "^4.0.6",
    "ts-node": "8.10.2"
  },
  "engines": {
    "node": ">=12.x"
  },
  "private": true
}
