{
  "version": 2,
  "name": "sample.cell.server",
  "alias": "cell.platform.uiharness.com",
  "env": {
    "PLATFORM_MONGO": "@platform-mongo"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/now.ts"
    }
  ],
  "builds": [
    {
      "src": "**/*.{ts,tsx}",
      "use": "@now/node",
      "config": {
        "includeFiles": ["*.yml", "*.json"]
      }
    }
  ]
}
