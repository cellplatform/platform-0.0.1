{
  "name": "@platform/npm.express",
  "version": "0.1.99",
  "description": "Express middleware for working with NPM.",
  "main": "./lib/index",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "node lib/server/start $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "dbuild": "sh sh/dbuild.sh",
    "dpush": "sh sh/dpush.sh",
    "drun": "cd examples && docker-compose up --force-recreate",
    "reset": "sh sh/reset.sh",
    "example": "sh sh/example.sh"
  },
  "dependencies": {
    "@platform/exec": "0.2.45",
    "@platform/npm": "0.2.51",
    "@types/express": "4.17.2",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "filesize": "6.0.1",
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "@platform/test": "0.1.2",
    "@platform/ts": "3.7.12",
    "@types/body-parser": "1.17.1",
    "@types/filesize": "5.0.0",
    "@types/minimist": "1.2.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
