{
  "name": "@platform/npm.express",
  "version": "0.1.159",
  "description": "Express middleware for working with NPM.",
  "main": "./lib/index",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "node lib/server/start $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "prepare": "ts prepare",
    "dbuild": "sh sh/dbuild.sh",
    "dpush": "sh sh/dpush.sh",
    "drun": "cd examples && docker-compose up --force-recreate",
    "reset": "sh sh/reset.sh",
    "example": "sh sh/example.sh"
  },
  "dependencies": {
    "@platform/exec": "0.3.2",
    "@platform/npm": "0.4.8",
    "@types/express": "4.17.13",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "filesize": "6.3.0",
    "minimist": "1.2.5",
    "semver": "7.3.5"
  },
  "devDependencies": {
    "@platform/test": "0.2.6",
    "@platform/ts": "4.1.13",
    "@types/body-parser": "1.19.1",
    "@types/filesize": "5.0.0",
    "@types/minimist": "1.2.2"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
