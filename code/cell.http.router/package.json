{
  "name": "@platform/cell.http.router",
  "version": "0.8.30",
  "description": "Isolated router logic for the Cell operating system HTTP server.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "cd test.browser && yarn start",
    "watch": "yarn start",
    "test": "ts test",
    "test:integration": "ts test --suffix INTEGRATION",
    "lint": "echo ts lint",
    "build": "ts build --no-esm",
    "clean": "ts-node -T scripts.ts/clean",
    "prepare": "ts-node -T scripts.ts/prepare && ts prepare && rm -rf tmp"
  },
  "dependencies": {
    "@platform/cache": "0.2.41",
    "@platform/cell.client": "0.3.9",
    "@platform/cell.db": "0.3.171",
    "@platform/cell.func": "0.1.248",
    "@platform/cell.schema": "0.5.19",
    "@platform/cell.types": "0.5.20",
    "@platform/cell.typesystem": "0.0.138",
    "@platform/cell.value": "0.2.158",
    "@platform/http": "0.14.0",
    "@platform/http.router": "0.0.45",
    "@platform/libs": "0.2.3",
    "@platform/log": "0.1.35",
    "@platform/util.value": "0.8.5",
    "cheerio": "1.0.0-rc.3",
    "mime-types": "2.1.27",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@platform/cell.fs.local": "0.4.141",
    "@platform/fsdb.nedb": "1.3.141",
    "@platform/micro": "0.15.0",
    "@platform/test": "0.1.17",
    "@platform/ts": "4.0.6",
    "@types/cheerio": "0.22.22",
    "@types/mime-types": "2.1.0",
    "@types/minimatch": "3.0.3",
    "form-data": "3.0.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
