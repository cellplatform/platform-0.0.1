  "dependencies": {
    "@platform/cache": "0.2.37",
    "@platform/cell.client": "0.2.162",
    "@platform/cell.db": "0.3.154",
    "@platform/cell.func": "0.1.231",
    "@platform/cell.schema": "0.5.2",
    "@platform/cell.types": "0.5.3",
    "@platform/cell.typesystem": "0.0.121",
    "@platform/cell.value": "0.2.141",
    "@platform/fsdb.types": "0.6.37",
    "@platform/http": "0.12.32",
    "@platform/http.router": "0.0.41",
    "@platform/libs": "0.2.2",
    "@platform/log": "0.1.31",
    "@platform/util.value": "0.8.1",
    "cheerio": "1.0.0-rc.3",
    "mime-types": "2.1.27",
    "minimatch": "3.0.4"
