  "devDependencies": {
    "@platform/cell.fs.local": "0.4.124",
    "@platform/fs": "0.5.24",
    "@platform/fsdb.nedb": "1.3.137",
    "@platform/micro": "0.14.52",
    "@platform/npm": "0.3.14",
    "@platform/test": "0.1.16",
    "@platform/ts": "4.0.2",
    "@types/cheerio": "0.22.22",
    "@types/mime-types": "2.1.0",
    "@types/minimatch": "3.0.3",
    "form-data": "3.0.0"
