{
  "name": "@platform/cell.fs.bus",
  "version": "0.0.12",
  "description": "Filesystem provided through implementation agnostic event-bus.",
  "sideEffects": false,
  "main": "lib/web/index",
  "types": "lib/web/index.d.ts",
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "test": "ts test $@",
    "test:integration": "ts test --suffix INTEGRATION",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "reset": "rm -rf tmp"
  },
  "dependencies": {
    "@platform/cell.fs": "0.4.228",
    "@platform/cell.fs.local": "0.4.228"
  },
  "devDependencies": {
    "@platform/test": "0.3.2",
    "@platform/ts": "4.7.7"
  },
  "files": [
    "lib",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
