{
  "name": "sys",
  "description": "Deployment of all main system libs",
  "version": "0.1.16",
  "main": "lib/index",
  "sideEffects": false,
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "test": "ts test $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "build.all": "sh script.sh/build-all.sh",
    "build.all.generate": "ts-node -T script.ts/generate.build-all.sh.ts",
    "bundle": "cell.compiler bundle --bump $@",
    "watch": "cell.compiler watch $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@",
    "deploy": "ts-node -T script.ts/vercel.run",
    "bbd": "yarn build.all && yarn bundle && yarn deploy"
  },
  "dependencies": {},
  "devDependencies": {
    "@platform/cell.compiler": "~0.4.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "license": "MIT",
  "private": true
}
