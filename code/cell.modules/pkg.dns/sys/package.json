{
  "name": "sys",
  "description": "Standard federated-function endpoing for 'system' modules",
  "version": "0.0.48",
  "main": "lib/index",
  "sideEffects": false,
  "compiler": {
    "port": 5050
  },
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "test": "ts test $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "bundle": "cell.compiler bundle --bump $@",
    "watch": "cell.compiler watch $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@",
    "deploy": "ts-node -T script.ts/deploy.vercel"
  },
  "dependencies": {},
  "devDependencies": {
    "@platform/cell.compiler": "~0.4.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "license": "MIT",
  "private": true
}
