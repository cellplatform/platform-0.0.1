{
  "name": "sys.scratchpad.node",
  "version": "0.0.3",
  "sideEffects": false,
  "scripts": {
    "start": "ts-node -T src/main/index",
    "dev": "yarn start",
    "test": "ts test $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "bundle": "cell.compiler bundle node $@",
    "watch": "cell.compiler watch node $@",
    "upload": "cell.compiler upload node $@",
    "info": "cell.compiler info node $@",
    "clean": "cell.compiler clean $@",
    "deploy": "cell.compiler upload node --bump --dir=sys.scratchpad.node $@"
  },
  "dependencies": {
    "@platform/log": "~0.1.60"
  },
  "devDependencies": {
    "@platform/cell.compiler": "~0.3.55"
  },
  "license": "MIT",
  "private": true
}
