{
  "name": "sys.ui.video",
  "version": "0.0.53",
  "sideEffects": false,
  "compiler": {
    "port": 3037
  },
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev esic --no-exports $@",
    "test": "ts test $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "bundle": "cell.compiler bundle esic $@",
    "watch": "cell.compiler watch $@",
    "upload": "cell.compiler upload $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve esic $@",
    "deploy": "cell.compiler upload --bump --dir sys.ui.video $@",
    "tmp": "ts-node -T tmp.projects/esic.w2/deploy"
  },
  "dependencies": {
    "@types/vimeo__player": "^2.10.0",
    "@vimeo/player": "^2.15.0",
    "core-js": "3.16.4"
  },
  "devDependencies": {
    "@platform/cell.compiler": "~0.4.1",
    "@types/dom-mediacapture-record": "^1.0.7",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "license": "MIT",
  "private": true
}
