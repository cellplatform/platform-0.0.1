{
  "name": "sys.scratchpad",
  "version": "0.0.1",
  "sideEffects": false,
  "compiler": {
    "port": 3033
  },
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "test": "ts test $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "cmd": "cell.compiler --help $@",
    "bundle": "cell.compiler bundle $@",
    "watch": "cell.compiler watch $@",
    "upload": "cell.compiler upload $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@",
    "deploy": "yarn upload --dir=sample --port 5000",
    "public": "ngrok http 5000 --hostname=os.ngrok.io"
  },
  "dependencies": {
    "d3": "^6.6.1",
    "peerjs": "^1.3.1"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.3.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "license": "MIT",
  "private": true
}
