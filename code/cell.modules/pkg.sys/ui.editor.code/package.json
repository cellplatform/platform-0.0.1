{
  "name": "ui.editor.code",
  "version": "0.0.0",
  "scripts": {
    "build": "ts build --no-esm $@",
    "lint": "ts lint $@",
    "dev": "cell.compiler dev $@",
    "bundle": "cell.compiler bundle $@",
    "bundle.worker": "cell.compiler bundle --config lib/config/compiler.webworker $@",
    "watch": "cell.compiler watch $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cd dist/web && serve -l 3002",
    "tmp": "cell.compiler $@"
  },
  "dependencies": {
    "monaco-editor": "^0.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.1.4"
  },
  "license": "MIT",
  "private": true
}
