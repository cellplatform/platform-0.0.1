{
  "name": "sys.ui.editor.code",
  "version": "0.0.0",
  "compiler": {
    "port": 3003
  },
  "scripts": {
    "build": "ts build --no-esm $@",
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "bundle": "cell.compiler bundle --name=prod $@",
    "watch": "cell.compiler watch $@",
    "upload": "cell.compiler upload $@",
    "clean": "sh script.sh/clean.sh $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@",
    "vs": "yarn cp && cd lib.monaco && serve -l 3004 --cors"
  },
  "dependencies": {
    "@monaco-editor/react": "^3.6.3",
    "monaco-editor": "^0.21.2"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.1.4",
    "sys.ui.harness": "^0.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "license": "MIT",
  "private": true
}
