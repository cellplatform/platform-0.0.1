  "dependencies": {
    "core-js": "^3.10.0",
    "@dnd-kit/core": "^2.1.2",
    "@dnd-kit/modifiers": "^1.0.5",
    "@dnd-kit/sortable": "^2.0.1",
    "@dnd-kit/utilities": "^1.0.3",
    "framer-motion": "^4.1.2"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.3.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "license": "MIT",
  "private": true
}
