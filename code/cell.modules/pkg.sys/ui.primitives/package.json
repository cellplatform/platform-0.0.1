    "@platform/ui.spinner": "^0.0.179",
    "@platform/ui.text": "^0.3.94",
    "@platform/ui.tree": "^0.8.26"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
