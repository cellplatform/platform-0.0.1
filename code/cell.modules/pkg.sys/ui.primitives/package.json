{
  "name": "sys.ui.primitives",
  "version": "0.0.83",
  "sideEffects": false,
  "compiler": {
    "port": 3036
  },
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "bundle": "cell.compiler bundle $@",
    "watch": "cell.compiler watch $@",
    "upload": "cell.compiler upload $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@",
    "deploy": "cell.compiler upload --bump --dir sys.ui.primitives $@"
  },
  "dependencies": {
    "@dnd-kit/core": "^3.0.3",
    "@dnd-kit/modifiers": "^2.0.0",
    "@dnd-kit/sortable": "^3.0.1",
    "@dnd-kit/utilities": "^2.0.0",
    "core-js": "3.12.1",
    "framer-motion": "^4.1.16",
    "qrcode.react": "^1.0.1"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.3.44",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@types/qrcode.react": "^1.0.1"
  },
  "license": "MIT",
  "private": true
}
