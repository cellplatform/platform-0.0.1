    "@platform/ui.spinner": "^0.0.165",
    "@platform/ui.text": "^0.3.80",
    "@platform/ui.tree": "^0.8.12",
    "preset.env": "^0.0.0"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "license": "MIT",
  "private": true
