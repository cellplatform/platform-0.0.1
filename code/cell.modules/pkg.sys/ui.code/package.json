{
  "name": "sys.ui.code",
  "version": "0.0.80",
  "compiler": {
    "port": 3034
  },
  "scripts": {
    "test": "ts test $@",
    "build": "ts build --no-esm $@",
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "lint": "ts lint $@",
    "bundle": "cell.compiler bundle $@",
    "watch": "cell.compiler watch $@",
    "upload": "cell.compiler upload $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@",
    "copy": "ts-node -T src/node/fs.copy/run",
    "deploy": "cell.compiler upload --dir sys.ui.code $@",
    "tmp": "ts-node -T src/node/tmp"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.0.11",
    "@platform/state": "^0.7.42",
    "monaco-editor": "^0.24.0",
    "monaco-themes": "^0.3.3"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.3.41",
    "@types/prettier": "^2.1.6",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "license": "MIT",
  "private": true
}
