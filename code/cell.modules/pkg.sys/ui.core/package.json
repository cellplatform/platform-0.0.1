    "@platform/ui.text": "^0.3.80",
    "@platform/ui.tree": "^0.8.12",
    "preset.react": "^0.0.0",
    "preset.env": "^0.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.1.4"
