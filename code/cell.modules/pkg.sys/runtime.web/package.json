{
  "name": "sys.runtime.web",
  "version": "0.0.1",
  "sideEffects": false,
  "compiler": {
    "port": 5051
  },
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "test": "ts test $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "bundle": "cell.compiler bundle --bump $@",
    "watch": "cell.compiler watch $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@"
  },
  "dependencies": {
    "@platform/cell.runtime.web": "0.1.72"
  },
  "devDependencies": {
    "@platform/cell.compiler": "~0.4.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "license": "MIT",
  "private": true
}
