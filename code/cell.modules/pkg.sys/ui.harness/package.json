{
  "name": "sys.ui.harness",
  "version": "0.0.0",
  "sideEffects": false,
  "scripts": {
    "build": "ts build --no-esm $@",
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "bundle": "cell.compiler bundle --name=prod $@",
    "watch": "cell.compiler watch $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cd dist/web && serve -l 3003"
  },
  "dependencies": {
    "preset.env": "^0.0.0",
    "sys.ui.primitives": "^0.0.0"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.1.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "license": "MIT",
  "private": true
}
