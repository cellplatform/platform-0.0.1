{
  "name": "sys.ui.harness",
  "version": "0.0.9",
  "sideEffects": false,
  "compiler": {
    "port": 3003
  },
  "scripts": {
    "build": "ts build --no-esm $@",
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports $@",
    "bundle": "cell.compiler bundle --name=prod $@",
    "watch": "cell.compiler watch $@",
    "clean": "cell.compiler clean $@",
    "upload": "cell.compiler upload $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@"
  },
  "dependencies": {
    "preset.env": "^0.0.0",
    "sys.ui.primitives": "^0.0.9"
  },
  "devDependencies": {
    "@platform/cell.compiler": "^0.2.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "license": "MIT",
  "private": true
}
