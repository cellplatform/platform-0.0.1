    "serve": "cell.compiler serve $@",
    "deploy": "cell.compiler upload --dir ui.runtime $@"
  },
  "dependencies": {},
  "devDependencies": {
    "@platform/cell.compiler": "~0.3.37",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
