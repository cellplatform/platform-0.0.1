{
  "name": "sys.net",
  "version": "0.0.64",
  "sideEffects": false,
  "compiler": {
    "port": 3040
  },
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports --name web.dev $@",
    "test": "ts test $@",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "bundle": "cell.compiler bundle $@",
    "watch": "cell.compiler watch $@",
    "upload": "cell.compiler upload $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@",
    "deploy": "cell.compiler upload --bump --dir sys.net $@"
  },
  "dependencies": {
    "automerge": "^0.14.2",
    "core-js": "^3.10.0",
    "peerjs": "^1.3.1"
  },
  "devDependencies": {
    "@platform/cell.compiler": "~0.3.11",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "license": "MIT",
  "private": true
}
