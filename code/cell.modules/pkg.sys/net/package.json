{
  "name": "sys.net",
  "version": "0.0.219",
  "sideEffects": false,
  "main": "lib/index",
  "compiler": {
    "port": 3040
  },
  "scripts": {
    "start": "cell.compiler dev $@",
    "dev": "cell.compiler dev --no-exports --name web.dev $@",
    "test": "echo no-op",
    "lint": "ts lint $@",
    "build": "ts build --no-esm $@",
    "bundle": "cell.compiler bundle --bump $@",
    "watch": "cell.compiler watch $@",
    "upload": "cell.compiler upload $@",
    "clean": "cell.compiler clean $@",
    "info": "cell.compiler info $@",
    "serve": "cell.compiler serve $@",
    "deploy": "ts-node -T script.ts/deploy.vercel.run"
  },
  "dependencies": {
    "core-js": "3.16.4",
    "peerjs": "1.3.2",
    "filesize": "8.0.0",
    "ua-parser-js": "1.0.2"
  },
  "devDependencies": {
    "@types/filesize": "5.0.0",
    "@types/ua-parser-js": "0.7.36",
    "@platform/cell.compiler": "~0.4.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "license": "MIT",
  "private": true
}
