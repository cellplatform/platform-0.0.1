{
  "name": "@platform/fs.s3",
  "version": "0.7.54",
  "description": "Filesystem like API into AWS S3 (and Digital Ocean Spaces)",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "test:integration": "ts test --suffix INTEGRATION",
    "prepare": "ts prepare",
    "tmp": "ts-node -T scripts/tmp"
  },
  "dependencies": {
    "@platform/fs": "0.6.13",
    "@platform/fs.s3.types": "0.3.17",
    "@platform/http": "0.14.19",
    "@platform/libs": "0.2.10",
    "@platform/log": "0.1.52",
    "@platform/types": "0.5.21",
    "@platform/util.mimetype": "0.1.38",
    "@platform/util.value": "0.9.0",
    "aws-sdk": "2.948.0",
    "form-data": "4.0.0"
  },
  "devDependencies": {
    "@platform/test": "0.2.6",
    "@platform/ts": "4.1.13",
    "@types/dotenv": "8.2.0",
    "dotenv": "10.0.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
