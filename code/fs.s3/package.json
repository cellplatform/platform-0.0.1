{
  "name": "@platform/fs.s3",
  "version": "0.7.48",
  "description": "Filesystem like API into AWS S3 (and Digital Ocean Spaces)",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "test:integration": "ts test --suffix INTEGRATION",
    "prepare": "ts prepare",
    "tmp": "ts-node -T scripts/tmp"
  },
  "dependencies": {
    "@platform/fs": "0.6.10",
    "@platform/fs.s3.types": "0.3.13",
    "@platform/http": "0.14.13",
    "@platform/libs": "0.2.9",
    "@platform/log": "0.1.48",
    "@platform/types": "0.5.18",
    "@platform/util.mimetype": "0.1.34",
    "@platform/util.value": "0.8.18",
    "aws-sdk": "2.874.0",
    "form-data": "4.0.0"
  },
  "devDependencies": {
    "@platform/test": "0.2.5",
    "@platform/ts": "4.1.9",
    "@types/dotenv": "8.2.0",
    "dotenv": "8.2.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
