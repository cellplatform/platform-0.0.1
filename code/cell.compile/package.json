{
  "name": "@platform/cell.compile",
  "version": "0.0.0",
  "description": "Code compilation tools for CellOS.",
  "scripts": {
    "start": "node .",
    "watch": "nodemon .",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "bundle": "parcel build src/demo/page.ts",
    "clean": "rm -rf dist && rm -rf .parcel-cache"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=4.x",
        "electron": ">=2.x"
      }
    },
    "browser": {
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "dependencies": {
    "@platform/cell.types": "0.2.14",
    "@platform/libs": "0.1.5",
    "parcel": "^2.0.0-alpha.3.2"
  },
  "devDependencies": {
    "@platform/test": "0.1.0",
    "@platform/ts": "3.7.4"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
