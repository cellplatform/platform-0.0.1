{
  "name": "@platform/cell.compiler",
  "version": "0.3.46",
  "description": "Code compilation and bundling.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell.compiler": "lib/node.cli/index.js"
  },
  "compiler": {
    "port": 5050
  },
  "scripts": {
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "bundle": "yarn cmd bundle $@",
    "watch": "yarn cmd watch $@",
    "start": "yarn cmd dev $@",
    "dev": "yarn cmd dev --no-exports $@",
    "upload": "yarn cmd upload $@",
    "serve": "yarn cmd serve $@",
    "info": "yarn cmd info $@",
    "clean": "rm -rf tmp && yarn cmd clean $@",
    "node": "ts-node -T script.ts/tmp.node.runtime $@",
    "cmd": "node lib/node.cli $@"
  },
  "dependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-modules-commonjs": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@platform/cell.module": "0.0.123",
    "@platform/cell.runtime.node": "0.0.58",
    "@platform/cell.types": "0.5.86",
    "@platform/css": "0.4.40",
    "@platform/libs": "0.2.10",
    "@platform/log": "0.1.52",
    "@platform/polyfill": "0.0.3",
    "@svgr/webpack": "5.5.0",
    "@types/jsonpath": "0.2.0",
    "@types/webpack-dev-server": "3.11.5",
    "babel-loader": "8.2.2",
    "core-js": "3.15.2",
    "css-loader": "6.1.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.2.12",
    "html-webpack-plugin": "5.3.2",
    "image-size": "1.0.0",
    "minimist": "1.2.5",
    "ora": "5.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semver": "^7.3.5",
    "serve": "12.0.0",
    "style-loader": "3.1.0",
    "webpack": "5.45.1",
    "webpack-dev-server": "3.11.2",
    "worker-loader": "3.0.8"
  },
  "devDependencies": {
    "@platform/cell.runtime.web": "0.1.58",
    "@platform/react": "0.4.73",
    "@platform/test": "0.2.6",
    "@platform/ts": "4.1.13",
    "@types/minimist": "1.2.2",
    "@types/qrcode": "1.4.1",
    "qrcode": "1.4.4"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
