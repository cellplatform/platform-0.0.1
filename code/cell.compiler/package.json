{
  "name": "@platform/cell.compiler",
  "version": "0.3.6",
  "description": "Code compilation and bundling.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell.compiler": "lib/node.bin/index.js"
  },
  "compiler": {
    "port": 5050
  },
  "scripts": {
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "cmd": "node lib/node.bin $@",
    "bundle": "yarn cmd bundle $@",
    "watch": "yarn cmd watch $@",
    "start": "yarn cmd dev $@",
    "dev": "yarn cmd dev --no-exports $@",
    "upload": "yarn cmd upload $@",
    "serve": "yarn cmd serve $@",
    "info": "yarn cmd info $@",
    "clean": "rm -rf tmp && yarn cmd clean $@",
    "node": "ts-node -T script.ts/tmp.node.runtime $@"
  },
  "dependencies": {
    "@babel/core": "7.13.13",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-transform-modules-commonjs": "7.13.8",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@platform/cell.module": "0.0.91",
    "@platform/cell.runtime.node": "0.0.27",
    "@platform/cell.types": "0.5.57",
    "@platform/css": "0.4.31",
    "@platform/libs": "0.2.7",
    "@platform/log": "0.1.46",
    "@platform/polyfill": "0.0.1",
    "@svgr/webpack": "5.5.0",
    "@types/jsonpath": "0.2.0",
    "@types/webpack-dev-server": "3.11.3",
    "babel-loader": "8.2.2",
    "css-loader": "5.2.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "image-size": "0.9.7",
    "minimist": "1.2.5",
    "ora": "5.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "serve": "11.3.2",
    "style-loader": "2.0.0",
    "webpack": "5.28.0",
    "webpack-dev-server": "3.11.2",
    "worker-loader": "3.0.8"
  },
  "devDependencies": {
    "@platform/cell.runtime.web": "0.1.27",
    "@platform/react": "0.4.64",
    "@platform/test": "0.2.3",
    "@platform/ts": "4.1.7",
    "@types/minimist": "1.2.1",
    "@types/qrcode": "1.4.0",
    "qrcode": "1.4.4"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
