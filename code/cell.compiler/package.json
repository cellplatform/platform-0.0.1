{
  "name": "@platform/cell.compiler",
  "version": "0.4.0",
  "description": "Code compilation and bundling.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell.compiler": "lib/api.cli/index.js"
  },
  "compiler": {
    "port": 5050
  },
  "scripts": {
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "bundle": "yarn cmd bundle $@",
    "watch": "yarn cmd watch $@",
    "start": "yarn cmd dev $@",
    "dev": "yarn cmd dev --no-exports $@",
    "upload": "yarn cmd upload $@",
    "serve": "yarn cmd serve $@",
    "info": "yarn cmd info $@",
    "clean": "rm -rf tmp && yarn cmd clean $@",
    "node": "ts-node -T script.ts/tmp.node.runtime $@",
    "cmd": "node lib/api.cli $@"
  },
  "dependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-modules-commonjs": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@platform/cell.fs.local": "^0.4.224",
    "@platform/cell.module": "0.0.136",
    "@platform/cell.runtime.node": "0.0.71",
    "@platform/cell.types": "0.5.99",
    "@platform/css": "0.4.50",
    "@platform/libs": "0.2.14",
    "@platform/log": "0.1.62",
    "@platform/polyfill": "0.0.7",
    "@svgr/webpack": "5.5.0",
    "@types/jsonpath": "0.2.0",
    "@types/webpack-dev-server": "4.0.3",
    "babel-loader": "8.2.2",
    "core-js": "3.16.4",
    "css-loader": "6.2.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.3.2",
    "html-webpack-plugin": "5.3.2",
    "minimist": "1.2.5",
    "ora": "5.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semver": "^7.3.5",
    "serve": "12.0.0",
    "style-loader": "3.2.1",
    "webpack": "5.51.1",
    "webpack-dev-server": "4.0.0",
    "worker-loader": "3.0.8"
  },
  "devDependencies": {
    "@platform/cell.runtime.web": "0.1.71",
    "@platform/react": "0.4.83",
    "@platform/test": "0.2.10",
    "@platform/ts": "4.1.23",
    "@types/minimist": "1.2.2",
    "@types/qrcode": "1.4.1",
    "qrcode": "1.4.4"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
