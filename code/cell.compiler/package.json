{
  "name": "@platform/cell.compiler",
  "version": "0.1.4",
  "description": "Code compilation and bundling.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell.compiler": "lib/node.bin/index.js"
  },
  "scripts": {
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "prepare": "ts prepare",
    "serve": "cd dist/web && serve --listen 1234 --cors",
    "cmd": "node lib/node.bin $@",
    "bundle": "yarn cmd bundle  $@",
    "watch": "yarn cmd watch --name dev $@",
    "start": "yarn cmd dev $@",
    "dev": "yarn cmd dev --no-exports $@",
    "upload": "yarn cmd upload  $@",
    "upload-sample": "yarn cmd upload --name=prod --host=localhost:5000 --uri=cell:ckg2nl70400001wethqd5e0ry:A1 --dir=foobar $@",
    "info": "yarn cmd info $@",
    "clean": "yarn cmd clean $@"
  },
  "dependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@platform/cell.module": "0.0.33",
    "@platform/cell.types": "0.5.4",
    "@platform/css": "^0.4.16",
    "@platform/libs": "0.2.2",
    "@platform/log": "0.1.31",
    "@platform/polyfill": "0.0.1",
    "@types/jsonpath": "0.2.0",
    "@types/webpack-dev-server": "3.11.0",
    "babel-loader": "8.1.0",
    "css-loader": "5.0.0",
    "file-loader": "6.1.1",
    "fork-ts-checker-webpack-plugin": "5.2.0",
    "html-webpack-plugin": "5.0.0-alpha.6",
    "mini-svg-data-uri": "^1.2.3",
    "minimist": "1.2.5",
    "style-loader": "2.0.0",
    "webpack": "5.1.3",
    "webpack-dev-server": "3.11.0"
  },
  "devDependencies": {
    "@platform/react": "0.4.49",
    "@platform/test": "0.1.16",
    "@platform/ts": "4.0.2",
    "@types/minimist": "1.2.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
