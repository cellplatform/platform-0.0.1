{
  "name": "@platform/cell.compiler",
  "version": "0.1.3",
  "description": "Code compilation and bundling.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell.compiler": "lib/bin/index.js"
  },
  "scripts": {
    "start": "yarn dev",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "prepare": "ts prepare",
    "serve": "cd dist && serve --listen 1234",
    "cmd": "node lib/bin $@",
    "dev": "yarn cmd dev --config lib/test/config/dev",
    "bundle": "yarn cmd bundle --config lib/test/config/prod",
    "watch": "yarn cmd watch --config lib/test/config/dev",
    "upload": "yarn cmd upload --config lib/test/config/prod",
    "info": "yarn cmd info --config lib/test/config/prod",
    "clean": "yarn cmd clean"
  },
  "dependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@platform/cell.module": "0.0.32",
    "@platform/cell.types": "0.5.3",
    "@platform/libs": "0.2.2",
    "@platform/log": "0.1.31",
    "@types/jsonpath": "0.2.0",
    "@types/webpack-dev-server": "3.11.0",
    "babel-loader": "8.1.0",
    "css-loader": "4.3.0",
    "fork-ts-checker-webpack-plugin": "5.2.0",
    "html-webpack-plugin": "4.5.0",
    "minimist": "^1.2.5",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.4",
    "webpack": "5.0.0",
    "webpack-dev-server": "3.11.0"
  },
  "devDependencies": {
    "@platform/test": "0.1.16",
    "@platform/ts": "4.0.2",
    "@types/minimist": "^1.2.0",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
