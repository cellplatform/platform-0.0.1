{
  "name": "@platform/cell.compiler",
  "version": "0.1.4",
  "description": "Code compilation and bundling.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell.compiler": "lib/node/bin/index.js"
  },
  "scripts": {
    "start": "yarn dev",
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "prepare": "ts prepare",
    "serve": "cd dist && serve --listen 1234",
    "cmd": "node lib/node/bin $@",
    "bundle": "yarn cmd bundle --config lib/test/compiler.config $@",
    "watch": "yarn cmd watch --config lib/test/compiler.config --name dev $@",
    "dev": "yarn cmd dev --config lib/test/compiler.config --name dev $@",
    "upload": "yarn cmd upload --config lib/test/compiler.config $@",
    "upload-sample": "yarn cmd upload --config lib/test/compiler.config --name=prod --host=localhost:5000 --uri=cell:ckg2nl70400001wethqd5e0ry:A1 --dir=foobar $@",
    "info": "yarn cmd info --config lib/test/compiler.config",
    "clean": "yarn cmd clean",
    "tmp": "ts-node -T src/test/script.tmp"
  },
  "dependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@platform/cell.module": "0.0.33",
    "@platform/cell.types": "0.5.4",
    "@platform/libs": "0.2.2",
    "@platform/log": "0.1.31",
    "@platform/polyfill": "0.0.1",
    "@types/jsonpath": "0.2.0",
    "@types/webpack-dev-server": "3.11.0",
    "babel-loader": "8.1.0",
    "css-loader": "4.3.0",
    "file-loader": "6.1.1",
    "fork-ts-checker-webpack-plugin": "5.2.0",
    "html-webpack-plugin": "4.5.0",
    "minimist": "1.2.5",
    "style-loader": "2.0.0",
    "webpack": "5.1.0",
    "webpack-dev-server": "3.11.0"
  },
  "devDependencies": {
    "@platform/test": "0.1.16",
    "@platform/ts": "4.0.2",
    "@types/minimist": "1.2.0",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
