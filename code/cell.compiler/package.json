{
  "name": "@platform/cell.compiler",
  "version": "0.2.39",
  "description": "Code compilation and bundling.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "cell.compiler": "lib/node.bin/index.js"
  },
  "compiler": {
    "port": 5050
  },
  "scripts": {
    "test": "ts test",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "prepare": "ts prepare",
    "cmd": "node lib/node.bin $@",
    "bundle": "yarn cmd bundle $@",
    "watch": "yarn cmd watch $@",
    "start": "yarn cmd dev $@",
    "dev": "yarn cmd dev --no-exports $@",
    "upload": "yarn cmd upload $@",
    "serve": "yarn cmd serve $@",
    "info": "yarn cmd info $@",
    "clean": "yarn cmd clean $@",
    "node": "ts-node -T script.ts/node $@"
  },
  "dependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@platform/cell.module": "0.0.76",
    "@platform/cell.runtime.node": "0.0.12",
    "@platform/cell.types": "0.5.44",
    "@platform/css": "0.4.24",
    "@platform/libs": "0.2.3",
    "@platform/log": "0.1.39",
    "@platform/polyfill": "0.0.1",
    "@svgr/webpack": "5.4.0",
    "@types/jsonpath": "0.2.0",
    "@types/webpack-dev-server": "3.11.1",
    "babel-loader": "8.2.2",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.0.4",
    "html-webpack-plugin": "5.0.0-alpha.14",
    "mini-svg-data-uri": "1.2.3",
    "minimist": "1.2.5",
    "ora": "5.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "serve": "11.3.2",
    "style-loader": "2.0.0",
    "webpack": "5.8.0",
    "webpack-dev-server": "3.11.0",
    "worker-loader": "3.0.5"
  },
  "devDependencies": {
    "@platform/cell.runtime.web": "0.1.12",
    "@platform/react": "0.4.57",
    "@platform/test": "0.2.0",
    "@platform/ts": "4.1.0",
    "@types/minimist": "1.2.1",
    "@types/qrcode": "1.3.5",
    "qrcode": "1.4.4"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
