{
  "name": "@platform/cell.router",
  "version": "0.8.98",
  "description": "Isolated router logic for the Cell operating system HTTP server.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "cd test.browser && yarn start",
    "watch": "yarn start",
    "test": "ts test",
    "test:integration": "ts test --suffix INTEGRATION",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "clean": "ts-node -T scripts.ts/clean",
    "prepare": "ts-node -T scripts.ts/prepare && ts prepare && rm -rf tmp"
  },
  "dependencies": {
    "@platform/cache": "0.3.11",
    "@platform/cell.client": "0.3.48",
    "@platform/cell.db": "0.3.210",
    "@platform/cell.func": "0.1.287",
    "@platform/cell.schema": "0.5.58",
    "@platform/cell.types": "0.5.57",
    "@platform/cell.typesystem": "0.0.177",
    "@platform/cell.value": "0.2.197",
    "@platform/http": "0.14.11",
    "@platform/http.router": "0.0.57",
    "@platform/libs": "0.2.7",
    "@platform/log": "0.1.46",
    "@platform/util.value": "0.8.16",
    "cheerio": "1.0.0-rc.5",
    "mime-types": "2.1.29",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@platform/cell.fs.local": "0.4.180",
    "@platform/cell.runtime.node": "0.0.27",
    "@platform/cell.runtime.web": "0.1.27",
    "@platform/fsdb.nedb": "1.3.153",
    "@platform/micro": "0.16.7",
    "@platform/test": "0.2.3",
    "@platform/ts": "4.1.7",
    "@types/cheerio": "0.22.24",
    "@types/mime-types": "2.1.0",
    "@types/minimatch": "3.0.3",
    "form-data": "4.0.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
