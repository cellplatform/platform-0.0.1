{
  "name": "@platform/cell.router",
  "version": "0.9.42",
  "description": "Isolated router logic for the Cell operating system HTTP server.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "ts test",
    "test:integration": "ts test --suffix INTEGRATION",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "clean": "ts-node -T scripts.ts/clean",
    "prepare": "ts-node -T scripts.ts/prepare && ts prepare && rm -rf tmp"
  },
  "dependencies": {
    "@platform/cache": "0.3.25",
    "@platform/cell.client": "0.3.88",
    "@platform/cell.db": "0.3.251",
    "@platform/cell.func": "0.1.327",
    "@platform/cell.schema": "0.5.98",
    "@platform/cell.types": "0.5.95",
    "@platform/cell.typesystem": "0.0.217",
    "@platform/cell.value": "0.2.237",
    "@platform/http": "0.14.27",
    "@platform/http.router": "0.0.73",
    "@platform/libs": "0.2.12",
    "@platform/log": "0.1.60",
    "@platform/util.value": "0.9.8",
    "cheerio": "1.0.0-rc.10",
    "mime-types": "2.1.31",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@platform/cell.fs.local": "0.4.220",
    "@platform/cell.runtime.node": "0.0.67",
    "@platform/cell.runtime.web": "0.1.67",
    "@platform/fsdb.nedb": "1.3.170",
    "@platform/micro": "0.16.23",
    "@platform/test": "0.2.8",
    "@platform/ts": "4.1.21",
    "@types/cheerio": "0.22.30",
    "@types/mime-types": "2.1.0",
    "@types/minimatch": "3.0.5",
    "form-data": "4.0.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
