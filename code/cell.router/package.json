{
  "name": "@platform/cell.router",
  "version": "0.8.87",
  "description": "Isolated router logic for the Cell operating system HTTP server.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "cd test.browser && yarn start",
    "watch": "yarn start",
    "test": "ts test",
    "test:integration": "ts test --suffix INTEGRATION",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "clean": "ts-node -T scripts.ts/clean",
    "prepare": "ts-node -T scripts.ts/prepare && ts prepare && rm -rf tmp"
  },
  "dependencies": {
    "@platform/cache": "0.3.7",
    "@platform/cell.client": "0.3.38",
    "@platform/cell.db": "0.3.200",
    "@platform/cell.func": "0.1.277",
    "@platform/cell.schema": "0.5.48",
    "@platform/cell.types": "0.5.48",
    "@platform/cell.typesystem": "0.0.167",
    "@platform/cell.value": "0.2.187",
    "@platform/http": "0.14.7",
    "@platform/http.router": "0.0.53",
    "@platform/libs": "0.2.4",
    "@platform/log": "0.1.42",
    "@platform/util.value": "0.8.12",
    "cheerio": "1.0.0-rc.3",
    "mime-types": "2.1.27",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@platform/cell.fs.local": "0.4.170",
    "@platform/cell.runtime.node": "0.0.17",
    "@platform/cell.runtime.web": "0.1.17",
    "@platform/fsdb.nedb": "1.3.149",
    "@platform/micro": "0.16.3",
    "@platform/test": "0.2.1",
    "@platform/ts": "4.1.3",
    "@types/cheerio": "0.22.23",
    "@types/mime-types": "2.1.0",
    "@types/minimatch": "3.0.3",
    "form-data": "3.0.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
