{
  "name": "@platform/cell.router",
  "version": "0.9.47",
  "description": "Isolated router logic for the Cell operating system HTTP server.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "ts test",
    "test:integration": "ts test --suffix INTEGRATION",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "clean": "ts-node -T scripts.ts/clean",
    "prepare": "ts-node -T scripts.ts/prepare && ts prepare && rm -rf tmp"
  },
  "dependencies": {
    "@platform/cache": "0.3.27",
    "@platform/cell.client": "0.3.93",
    "@platform/cell.db": "0.3.256",
    "@platform/cell.func": "0.1.332",
    "@platform/cell.schema": "0.5.103",
    "@platform/cell.types": "0.5.100",
    "@platform/cell.typesystem": "0.0.222",
    "@platform/cell.value": "0.2.242",
    "@platform/http": "0.14.29",
    "@platform/http.router": "0.0.75",
    "@platform/libs": "0.2.14",
    "@platform/log": "0.1.62",
    "@platform/util.value": "0.9.10",
    "cheerio": "1.0.0-rc.10",
    "mime-types": "2.1.32",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@platform/cell.fs.local": "0.4.225",
    "@platform/cell.runtime.node": "0.0.72",
    "@platform/cell.runtime.web": "0.1.72",
    "@platform/fsdb.nedb": "1.3.172",
    "@platform/micro": "0.16.25",
    "@platform/test": "0.2.10",
    "@platform/ts": "4.1.23",
    "@types/cheerio": "0.22.30",
    "@types/mime-types": "2.1.1",
    "@types/minimatch": "3.0.5",
    "form-data": "4.0.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
