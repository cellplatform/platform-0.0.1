{
  "name": "@platform/cell.router",
  "version": "0.8.71",
  "description": "Isolated router logic for the Cell operating system HTTP server.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "cd test.browser && yarn start",
    "watch": "yarn start",
    "test": "ts test",
    "test:integration": "ts test --suffix INTEGRATION",
    "lint": "ts lint",
    "build": "ts build --no-esm",
    "clean": "ts-node -T scripts.ts/clean",
    "prepare": "ts-node -T scripts.ts/prepare && ts prepare && rm -rf tmp",
    "tmp": "rm -rf tmp"
  },
  "dependencies": {
    "@platform/cache": "0.3.4",
    "@platform/cell.client": "0.3.23",
    "@platform/cell.db": "0.3.185",
    "@platform/cell.func": "0.1.262",
    "@platform/cell.schema": "0.5.33",
    "@platform/cell.types": "0.5.34",
    "@platform/cell.typesystem": "0.0.152",
    "@platform/cell.value": "0.2.172",
    "@platform/http": "0.14.4",
    "@platform/http.router": "0.0.50",
    "@platform/libs": "0.2.3",
    "@platform/log": "0.1.39",
    "@platform/util.value": "0.8.9",
    "cheerio": "1.0.0-rc.3",
    "mime-types": "2.1.27",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@platform/cell.fs.local": "0.4.155",
    "@platform/cell.runtime.node": "0.0.2",
    "@platform/cell.runtime.web": "0.1.2",
    "@platform/fsdb.nedb": "1.3.146",
    "@platform/micro": "0.16.0",
    "@platform/test": "0.2.0",
    "@platform/ts": "4.1.0",
    "@types/cheerio": "0.22.22",
    "@types/mime-types": "2.1.0",
    "@types/minimatch": "3.0.3",
    "form-data": "3.0.0"
  },
  "files": [
    "lib",
    "static",
    "!lib/test",
    "!lib/tests",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
