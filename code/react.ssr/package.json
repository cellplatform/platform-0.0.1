{
  "name": "@platform/react.ssr",
  "version": "0.2.0",
  "description": "An SSR (server-side-render) server for react apps bundled with ParcelJS and hosted on S3.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "ui": "ui $@",
    "dev": "ui start web $@",
    "start": "node lib.test/test/server",
    "watch": "nodemon lib.test/test/server",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "build-test": "ts build --tsconfig tsconfig.test.json",
    "prepare": "ts prepare",
    "bundle": "ui bundle web && yarn prep",
    "prep": "ts-node -T scripts/prep",
    "pb": "ts-node -T scripts/push.bundle",
    "pm": "ts-node -T scripts/push.manifest"
  },
  "dependencies": {
    "@platform/fs": "0.1.7",
    "@platform/fs.s3": "0.2.4",
    "@platform/http": "0.2.13",
    "@platform/libs": "0.1.0",
    "@platform/log": "0.0.135",
    "@platform/npm": "0.2.1",
    "@platform/react": "0.3.14",
    "bowser": "2.5.3",
    "cheerio": "1.0.0-rc.3",
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "helmet": "3.20.0",
    "listr": "0.14.3",
    "path-to-regexp": "3.0.0"
  },
  "devDependencies": {
    "@platform/test": "0.0.60",
    "@platform/ts": "3.4.35",
    "@types/bowser": "1.1.1",
    "@types/cheerio": "0.22.13",
    "@types/cors": "2.8.5",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.0",
    "@types/helmet": "0.0.43",
    "@uiharness/dev": "0.8.7"
  },
  "files": [
    "lib"
  ],
  "license": "MIT"
}
