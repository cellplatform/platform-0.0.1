{
  "version": 2,
  "name": "sample.ssr.server",
  "alias": "ssr.platform.uiharness.com",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/now.ts"
    }
  ],
  "builds": [
    {
      "src": "**/*.{ts,tsx}",
      "use": "@now/node",
      "config": {
        "includeFiles": ["*.yml"]
      }
    }
  ]
}
