{
  "name": "@platform/cell.compile.react",
  "version": "0.0.33",
  "description": "CellOS code compilation toolchain for React.",
  "scripts": {
    "start": "parcel demo/react/index.html",
    "watch": "yarn start",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "clean": "sh sh/clean.sh",
    "bundle": "sh sh/bundle.sh"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=4.x",
        "electron": ">=7.x"
      }
    },
    "browser": {
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "dependencies": {
    "@platform/cell.types": "0.4.8",
    "@platform/libs": "0.2.0",
    "@platform/log": "^0.1.4",
    "@platform/react": "^0.4.0",
    "@platform/ui.spinner": "^0.0.116",
    "parcel": "^1.12.4"
  },
  "devDependencies": {
    "@platform/test": "0.1.2",
    "@platform/ts": "3.7.10"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "license": "MIT"
}
