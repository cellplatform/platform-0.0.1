{
  "name": "@platform/cell.compile.react",
  "version": "0.0.11",
  "description": "CellOS code compilation toolchain for React.",
  "scripts": {
    "start": "parcel demo/hello-world/src/index.html",
    "watch": "yarn start",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build",
    "prepare": "ts prepare",
    "update": "sh sh/update.sh",
    "bundle": "ts-node -T scripts/bundle.ts",
    "clean": "sh sh/clean.sh"
  },
  "dependencies": {
    "@platform/cell.types": "0.4.17",
    "@platform/cli": "0.5.12",
    "@platform/cli.prompt": "^0.2.13",
    "@platform/libs": "0.2.0"
  },
  "devDependencies": {
    "@platform/test": "0.1.2",
    "@platform/ts": "3.7.12"
  },
  "files": [
    "lib",
    "!lib/test",
    "!*.TEST.{js, mjs, d.ts}",
    "!*.INTEGRATION.{js, mjs, d.ts}"
  ],
  "repository": "uiharness/platform",
  "license": "MIT"
}
